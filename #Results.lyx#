\begin_layout Section
4d 
\begin_inset Formula $\N=4$
\end_inset


\end_layout

\begin_layout Standard
We compute the partition function 
\begin_inset Formula $Z_{\left(n_{1},n_{2}\right)}$
\end_inset

 for the 4d 
\begin_inset Formula $\N=4$
\end_inset

 
\begin_inset Formula $\SU(2)$
\end_inset

 SYM theory.
\end_layout

\begin_layout Subsection
\begin_inset Formula $\left(n_{1},n_{2}\right)=\left(0,n\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{\left(0,0\right)}=\frac{\eta\left(q\right)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}}{\theta_{1}(x)\theta_{1}(fh)\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Z_{\left(0,2\right)} & =\frac{2\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)^{2}\theta_{1}\left(\frac{x}{fh}\right)^{2}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{4}}{\theta_{1}(x)\theta_{1}(fh)^{3}\theta_{1}\left(\frac{h}{x}\right)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{5}}+\frac{\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)^{2}\theta_{1}\left(\frac{f}{x}\right)^{2}\theta_{1}\left(\frac{x}{fh}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2}}{\theta_{1}(x)^{3}\theta_{1}(fh)^{3}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{3}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
Z_{\left(0,4\right)} & =+\frac{\blue 2\red{\eta(q)^{3}}\green{\theta_{1}\left(\frac{1}{h}\right)^{4}}\red{\theta_{1}\left(\frac{fh}{x}\right)^{2}}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{4}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{8}}{\blue{\theta_{1}(x)}\green{\theta_{1}(fh)^{5}}\theta_{1}\left(\frac{h}{x}\right)^{4}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{9}}+\frac{\blue 2\red{\eta(q)^{3}}\green{\theta_{1}\left(\frac{1}{h}\right)^{4}}\theta_{1}\left(\frac{f}{x}\right)^{2}\red{\theta_{1}\left(\frac{fh}{x}\right)^{2}}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{6}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2}}{\blue{\theta_{1}(x)^{3}}\green{\theta_{1}(fh)^{5}}\theta_{1}\left(\frac{h}{x}\right)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{7}}+\frac{\red{\eta(q)^{3}}\green{\theta_{1}\left(\frac{1}{h}\right)^{4}}\theta_{1}\left(\frac{f}{x}\right)^{4}\red{\theta_{1}\left(\frac{fh}{x}\right)^{2}}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{4}}{\blue{\theta_{1}(x)^{5}}\green{\theta_{1}(fh)^{5}}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{5}}
\end{align*}

\end_inset


\begin_inset Formula 
\[
Z_{\left(0,n\right)}=\frac{\red{\eta(q)^{3}}\red{\theta_{1}\left(\frac{fh}{x}\right)^{2}}\green{\theta_{1}\left(\frac{1}{h}\right)^{n}}}{\green{\theta_{1}(fh)^{n+1}}}\sum_{k=0}^{\frac{n}{2}}\frac{\blue{\left(2-\delta_{k,\frac{n}{2}}\right)}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n-2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2k}}{\blue{\theta_{1}(x)^{2k+1}}\theta_{1}\left(\frac{h}{x}\right)^{n-2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2k+1}}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
Z_{\left(0,n\right)} & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\sum_{k=0}^{\frac{n}{2}}\frac{\left(2-\delta_{k,\frac{n}{2}}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n-2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2k}}{\theta_{1}(x)^{2k+1}\theta_{1}\left(\frac{h}{x}\right)^{n-2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2k+1}}\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\sum_{k=0}^{\frac{n}{2}}\frac{\left(2-\delta_{k,\frac{n}{2}}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n-2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2k}}{\theta_{1}(x)^{2k+1}\theta_{1}\left(\frac{h}{x}\right)^{n-2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2k+1}}\\
\left\{ k\to\frac{n}{2}-k\right\}  & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\sum_{k=0}^{\frac{n}{2}}\frac{\left(2-\delta_{\frac{n}{2}-k,\frac{n}{2}}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n-2\left(\frac{n}{2}-k\right)}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2\left(\frac{n}{2}-k\right)}}{\theta_{1}(x)^{2\left(\frac{n}{2}-k\right)+1}\theta_{1}\left(\frac{h}{x}\right)^{n-2\left(\frac{n}{2}-k\right)}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2\left(\frac{n}{2}-k\right)+1}}\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\sum_{k=0}^{\frac{n}{2}}\frac{\left(2-\delta_{k,0}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2k}}{\theta_{1}(x)^{n-2k+1}\theta_{1}\left(\frac{h}{x}\right)^{2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2k}}\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\sum_{k=0}^{\frac{n}{2}}\frac{\left(2-\delta_{k,0}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2k}}{\theta_{1}(x)^{-2k}\theta_{1}\left(\frac{h}{x}\right)^{2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2k}}\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(1+2\sum_{k=1}^{\frac{n}{2}}\frac{\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2k}}{\theta_{1}(x)^{-2k}\theta_{1}\left(\frac{h}{x}\right)^{2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2k}}\right)\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(1+2\sum_{k=1}^{\frac{n}{2}}\left[\frac{\theta_{1}\left(x\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)\theta_{1}\left(\frac{x}{fh^{2}}\right)}{\theta_{1}\left(\frac{h}{x}\right)\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)}\right]^{2k}\right)\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(1+2\frac{A^{2}\left(-1+A^{n}\right)}{-1+A^{2}}\right)\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(\frac{-1+A^{2}+2A^{2}\left(-1+A^{n}\right)}{-1+A^{2}}\right)\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(\frac{-1-A^{2}+2A^{n+2}}{-1+A^{2}}\right)\\
 & =\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(\frac{-1-A^{2}+2A^{n+2}}{-1+A^{2}}\right)
\end{align*}

\end_inset

for,
\begin_inset Formula 
\begin{align*}
A & =\frac{\theta_{1}\left(x\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)\theta_{1}\left(\frac{x}{fh^{2}}\right)}{\theta_{1}\left(\frac{h}{x}\right)\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)}
\end{align*}

\end_inset

Now,
 for 
\begin_inset Formula $n\in2\mathbb{Z}+1$
\end_inset

,
 we have:
\begin_inset Formula 
\[
Z_{\left(0,1\right)}=\frac{\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)\theta_{1}\left(\frac{f}{x}\right)\theta_{1}\left(\frac{x}{fh^{2}}\right)\theta_{1}\left(\frac{x}{fh}\right)^{2}}{\theta_{1}(x)^{2}\theta_{1}(fh)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(0,3\right)}=\frac{2\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)^{3}\theta_{1}\left(\frac{f}{x}\right)\theta_{1}\left(\frac{x}{fh}\right)^{2}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{5}}{\theta_{1}(x)^{2}\theta_{1}(fh)^{4}\theta_{1}\left(\frac{h}{x}\right)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{6}}+\frac{\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)^{3}\theta_{1}\left(\frac{f}{x}\right)^{3}\theta_{1}\left(\frac{x}{fh}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{3}}{\theta_{1}(x)^{4}\theta_{1}(fh)^{4}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{4}}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(0,5\right)}=\frac{\blue 2\red{\eta(q)^{3}}\green{\theta_{1}\left(\frac{1}{h}\right)^{5}}\theta_{1}\left(\frac{f}{x}\right)\red{\theta_{1}\left(\frac{x}{fh}\right)^{2}}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{4}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{9}}{\blue{\theta_{1}(x)^{2}}\green{\theta_{1}}\green{(fh)^{6}}\theta_{1}\left(\frac{h}{x}\right)^{4}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{10}}+\frac{\blue 2\red{\eta(q)^{3}}\green{\theta_{1}\left(\frac{1}{h}\right)^{5}}\theta_{1}\left(\frac{f}{x}\right)^{3}\red{\theta_{1}\left(\frac{x}{fh}\right)^{2}}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{7}}{\blue{\theta_{1}(x)^{4}}\green{\theta_{1}(fh)^{6}}\theta_{1}\left(\frac{h}{x}\right)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{8}}+\frac{\red{\eta(q)^{3}}\green{\theta_{1}\left(\frac{1}{h}\right)^{5}}\theta_{1}\left(\frac{f}{x}\right)^{5}\red{\theta_{1}\left(\frac{x}{fh}\right)^{2}}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{5}}{\blue{\theta_{1}(x)^{6}}\green{\theta_{1}(fh)^{6}}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{6}}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
Z_{\left(0,1\right)} & =\frac{\red{\eta(q)^{3}}\red{\theta_{1}\left(\frac{fh}{x}\right)^{2}}\green{\theta_{1}\left(\frac{1}{h}\right)^{n}}}{\green{\theta_{1}(fh)^{n+1}}}\sum_{k=1}^{\frac{n+1}{2}}\frac{\blue{\left(2-\delta_{k,\frac{n+1}{2}}\right)}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n+1-2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2k+1}}{\blue{\theta_{1}(x)^{2k}}\theta_{1}\left(\frac{h}{x}\right)^{n+1-2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2k+2}}\\
 & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\sum_{k=1}^{\frac{n+1}{2}}\frac{\left(2-\delta_{k,\frac{n+1}{2}}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n+1-2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2k+1}}{\theta_{1}(x)^{2k}\theta_{1}\left(\frac{h}{x}\right)^{n+1-2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2k+2}}\\
\left\{ k\to\frac{n+1}{2}-k\right\} \to & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\sum_{k=0}^{\frac{n-1}{2}}\frac{\left(2-\delta_{k,0}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{n+1-2\left(\frac{n+1}{2}-k\right)}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2n-2\left(\frac{n+1}{2}-k\right)+1}}{\theta_{1}(x)^{2\left(\frac{n+1}{2}-k\right)}\theta_{1}\left(\frac{h}{x}\right)^{n+1-2\left(\frac{n+1}{2}-k\right)}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2n-2\left(\frac{n+1}{2}-k\right)+2}}\\
 & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\sum_{k=0}^{\frac{n-1}{2}}\frac{\left(2-\delta_{k,0}\right)\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{n+2k}}{\theta_{1}(x)^{n+1-2k}\theta_{1}\left(\frac{h}{x}\right)^{2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1+2k}}\\
 & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}\theta_{1}(x)^{n+1}}\frac{\theta_{1}\left(\frac{x}{fh^{2}}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\left(\sum_{k=0}^{\frac{n-1}{2}}\frac{\left(2-\delta_{k,0}\right)\theta_{1}(x)^{2k}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2k}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2k}}{\theta_{1}\left(\frac{h}{x}\right)^{2k}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2k}}\right)\\
 & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}\theta_{1}(x)^{n+1}}\frac{\theta_{1}\left(\frac{x}{fh^{2}}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\left(1+2\sum_{k=1}^{\frac{n-1}{2}}\left[\frac{\theta_{1}(x)\theta_{1}\left(\frac{x}{f^{2}h}\right)\theta_{1}\left(\frac{x}{fh^{2}}\right)}{\theta_{1}\left(\frac{h}{x}\right)\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)}\right]^{2k}\right)\\
 & =\frac{\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}\theta_{1}(x)^{n+1}}\frac{\theta_{1}\left(\frac{x}{fh^{2}}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\left(\frac{-1-A^{2}+2A^{n+1}}{-1+A^{2}}\right)
\end{align*}

\end_inset

Overall,
\begin_inset Formula 
\[
Z_{\left(0,n\right)}=\oint_{x=1}\frac{\d x}{x}\left[\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(h\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{fh^{2}}{x}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(\frac{-1-A^{2}+2A^{n+2}}{-1+A^{2}}\right)\right],n\in2\mathbb{Z}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(0,n\right)}=\oint_{x=1}\frac{\d x}{x}\left[\eta(q)^{3}\theta_{1}\left(\frac{fh}{x}\right)^{2}\frac{\theta_{1}\left(\frac{1}{h}\right)^{n}}{\theta_{1}(fh)^{n+1}}\frac{\theta_{1}\left(\frac{x}{fh^{2}}\right)^{n}}{\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{n+1}}\frac{1}{\theta_{1}(x)^{n+1}}\left(\frac{-1-A^{2}+2A^{n+1}}{-1+A^{2}}\right)\right],n\in2\mathbb{Z}+1
\]

\end_inset


\end_layout

\begin_layout Subsection
Modular properties
\end_layout

\begin_layout Standard
Here we check the modular properties of the various partition functions we've got.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Z_{\left(0,0\right)} & \overset{\tau\to\tau+1}{\longrightarrow}Z_{\left(0,0\right)}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
Z_{\left(0,0\right)}\left(u,v,\chi|\tau\right) & =\frac{\eta\left(q\right)^{3}\theta_{1}\left(u+v-\chi\right)^{2}}{\theta_{1}(\chi)\theta_{1}(u+v)\theta_{1}\left(2u+2v-\chi\right)}\\
 & \overset{\tau\to-\frac{1}{\tau}}{\longrightarrow}Z_{\left(0,0\right)}\left(\frac{u}{\tau},\frac{v}{\tau}\Big|-\frac{1}{\tau}\right)\\
 & =\oint_{\chi=0}\d\chi Z_{\left(0,0\right)}\left(\frac{u}{\tau},\frac{v}{\tau},\chi\Big|-\frac{1}{\tau}\right)\\
 & =\oint_{\chi=0}\d\chi\frac{1}{\tau}Z_{\left(0,0\right)}\left(\frac{u}{\tau},\frac{v}{\tau},\frac{\chi}{\tau}\Big|-\frac{1}{\tau}\right)
\end{align*}

\end_inset


\begin_inset Formula $ $
\end_inset


\begin_inset Formula 
\begin{align*}
Z_{\left(0,0\right)}\left(u,v,\chi|\tau\right) & =\frac{\eta\left(q\right)^{3}\theta_{1}\left(u+v-\chi\right)^{2}}{\theta_{1}(\chi)\theta_{1}(u+v)\theta_{1}\left(2u+2v-\chi\right)}\\
 & \overset{\tau\to-\frac{1}{\tau}}{\longrightarrow}\frac{1}{\tau}Z_{\left(0,0\right)}\left(\frac{u}{\tau},\frac{v}{\tau},\frac{\chi}{\tau}\Big|-\frac{1}{\tau}\right)\\
 & =\frac{1}{\tau}\frac{\sqrt{-i\tau}^{3}\left(-i\right)^{2}\sqrt{-i\tau}^{2}e^{\frac{2\pi i}{\tau}\left(u+v-\chi\right)^{2}}}{\left(-i\right)\sqrt{-i\tau}e^{\frac{\pi i}{\tau}\chi^{2}}\left(-i\right)\sqrt{-i\tau}e^{\frac{\pi i}{\tau}\left(u+v\right)^{2}}\left(-i\right)\sqrt{-i\tau}e^{\frac{\pi i}{\tau}\left(2u+2v-\chi\right)^{2}}}Z_{\left(0,0\right)}\\
 & =\frac{e^{\frac{2\pi i}{\tau}\left(u+v-\chi\right)^{2}}}{e^{\frac{\pi i}{\tau}\chi^{2}}e^{\frac{\pi i}{\tau}\left(u+v\right)^{2}}e^{\frac{\pi i}{\tau}\left(2u+2v-\chi\right)^{2}}}Z_{\left(0,0\right)}\\
 & =\exp\left(\frac{\pi i}{\tau}\left[2\left(u+v-\chi\right)^{2}-\chi^{2}-\left(u+v\right)^{2}-\left(2u+2v-\chi\right)^{2}\right]\right)Z_{\left(0,0\right)}\\
 & =\exp\left(\frac{\pi i}{\tau}\left[2\left(u+v\right)^{2}-4\chi\left(u+v\right)+2\chi^{2}-\chi^{2}-\left(u+v\right)^{2}-4\left(u+v\right)^{2}+4\chi\left(u+v\right)-\chi^{2}\right]\right)Z_{\left(0,0\right)}\\
 & =\exp\left(\frac{\pi i}{\tau}\left[2\left(u+v\right)^{2}-\bcancel{4\chi\left(u+v\right)}+\cancel{2\chi^{2}}-\cancel{\chi^{2}}-\left(u+v\right)^{2}-4\left(u+v\right)^{2}+\bcancel{4\chi\left(u+v\right)}-\cancel{\chi^{2}}\right]\right)Z_{\left(0,0\right)}\\
 & =\exp\left(-3\frac{\pi i}{\tau}\left(u+v\right)^{2}\right)Z_{\left(0,0\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Z_{\left(0,2\right)} & =\frac{2\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)^{2}\theta_{1}\left(\frac{x}{fh}\right)^{2}\theta_{1}\left(\frac{x}{f^{2}h}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{4}}{\theta_{1}(x)\theta_{1}(fh)^{3}\theta_{1}\left(\frac{h}{x}\right)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{5}}+\frac{\eta(q)^{3}\theta_{1}\left(\frac{1}{h}\right)^{2}\theta_{1}\left(\frac{f}{x}\right)^{2}\theta_{1}\left(\frac{x}{fh}\right)^{2}\theta_{1}\left(\frac{x}{fh^{2}}\right)^{2}}{\theta_{1}(x)^{3}\theta_{1}(fh)^{3}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{3}}\\
 & =A+B
\end{align*}

\end_inset


\begin_inset Formula 
\[
A\overset{\tau\to\tau+1}{\longrightarrow}\frac{e^{\frac{\pi i}{4}}\left(e^{\frac{\pi i}{4}}\right)^{10}}{\left(e^{\frac{\pi i}{4}}\right)^{11}}A=A
\]

\end_inset


\begin_inset Formula 
\begin{align*}
B & \overset{\tau\to\tau+1}{\longrightarrow}\frac{e^{\frac{\pi i}{4}}\left(e^{\frac{\pi i}{4}}\right)^{8}}{\left(e^{\frac{\pi i}{4}}\right)^{9}}B=B
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
A & \overset{\tau\to-\frac{1}{\tau}}{\longrightarrow}\frac{1}{\tau}\frac{\sqrt{-i\tau}^{3+10}\left(-i\right)^{10}}{\sqrt{-i\tau}^{11}\left(-i\right)^{11}}\exp\left(\frac{i\pi}{\tau}\left[-3\left(3u^{2}+6uv+v^{2}\right)\right]\right)A
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
B & \overset{\tau\to-\frac{1}{\tau}}{\longrightarrow}\frac{1}{\tau}\frac{\sqrt{-i\tau}^{3+8}\left(-i\right)^{8}}{\sqrt{-i\tau}^{9}\left(-i\right)^{9}}\exp\left(\frac{i\pi}{\tau}\left[-3\left(3u^{2}+6uv+v^{2}\right)\right]\right)B
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Jacobi Modular Forms
\end_layout

\begin_layout Standard
Jacobi modular forms are defined by
\begin_inset Formula 
\[
\phi\left(\frac{A\tau+B}{C\tau+D},\frac{z}{C\tau+D}\right)=\left(C\tau+D\right)^{k}e^{\frac{2\pi imCz^{2}}{C\tau+D}}\phi\left(\tau,z\right)
\]

\end_inset

where 
\begin_inset Formula $k$
\end_inset

 is the 
\begin_inset Quotes eld
\end_inset

weight
\begin_inset Quotes erd
\end_inset

 and 
\begin_inset Formula $m$
\end_inset

 the 
\begin_inset Quotes eld
\end_inset

index
\begin_inset Quotes erd
\end_inset

.
 They also obey
\begin_inset Formula 
\[
\phi\left(\tau,z+\lambda\tau+\mu\right)=e^{-2\pi im\left(\lambda^{2}\tau+2\lambda z\right)}\phi\left(\tau,z\right).
\]

\end_inset

We can span Jacobi modular forms using sums of 
\begin_inset Formula 
\[
M\left(\tau,z\right)=E_{4}^{a}\left(\tau\right)E_{6}^{b}\left(\tau\right)\phi_{-2,1}^{\alpha}\left(\tau,z\right)\phi_{0,1}^{\beta}\left(\tau,z\right)
\]

\end_inset

where 
\begin_inset Formula $a,b,\alpha,\beta$
\end_inset

 are non-negative integers.
 This form gives the modular properties under 
\begin_inset Formula $\text{SL}\left(2,\mathbb{Z}\right)$
\end_inset


\begin_inset Formula 
\[
M\left(\frac{A\tau+B}{C\tau+D},\frac{z}{C\tau+D}\right)=\left(C\tau+D\right)^{4a+6b-2\alpha}e^{\frac{2\pi i\left(\alpha+\beta\right)Cz^{2}}{C\tau+D}}M\left(\tau,z\right)
\]

\end_inset

where weight 
\begin_inset Formula $k$
\end_inset

 and index 
\begin_inset Formula $m$
\end_inset


\begin_inset Formula 
\[
k=4a+6b-2\alpha
\]

\end_inset


\begin_inset Formula 
\[
m=\alpha+\beta
\]

\end_inset

in this basis,
 the other transformation gives (probably,
 didn't check)
\begin_inset Formula 
\[
M\left(\tau,z+\lambda\tau+\mu\right)=e^{-2\pi i\left(\alpha+\beta\right)\left(\lambda^{2}\tau+2\lambda z\right)}M\left(\tau,z\right)
\]

\end_inset


\end_layout

\begin_layout Section
Getting rank 1 from modular properties
\end_layout

\begin_layout Standard
We already know that for any 
\begin_inset Formula $n\in\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
Z_{n}=\frac{2\left\lfloor \frac{n}{2}\right\rfloor +1}{\left(2\pi\right)^{2}}\frac{\theta_{1}\left(y\right)}{\eta\left(q\right)}\left(\frac{\theta_{1}\left(y^{2}\right)}{\eta\left(q\right)}\right)^{2n}
\]

\end_inset

we know from this result that the modular properties are
\begin_inset Formula 
\[
Z_{n}\left(\tau+1|u\right)=e^{i\frac{\pi}{6}\left(2n+1\right)}Z_{n}\left(\tau|u\right)
\]

\end_inset


\begin_inset Formula 
\[
Z_{n}\left(-\frac{1}{\tau}|\frac{u}{\tau}\right)=\left(-i\right)^{2n+1}e^{\frac{\pi i\left(8n+1\right)u^{2}}{\tau}}Z_{n}\left(\tau|u\right)
\]

\end_inset

we can try and match to the general formula
\begin_inset Formula 
\[
M\left(\frac{A\tau+B}{C\tau+D},\frac{z}{C\tau+D}\right)=\left(C\tau+D\right)^{4a+6b-2\alpha}e^{\frac{2\pi i\left(\alpha+\beta\right)Cz^{2}}{C\tau+D}}M\left(\tau,z\right)
\]

\end_inset

for 
\begin_inset Formula $\tau\to\tau+1$
\end_inset

 we have 
\begin_inset Formula $A=B=D=1$
\end_inset

 and 
\begin_inset Formula $C=0$
\end_inset


\begin_inset Formula 
\[
M\left(\tau+1|z\right)=M\left(\tau,z\right)
\]

\end_inset

for 
\begin_inset Formula $\tau\to-\frac{1}{\tau}$
\end_inset

 we have 
\begin_inset Formula $A=D=0$
\end_inset

,
 
\begin_inset Formula $B=-1,C=1$
\end_inset


\begin_inset Formula 
\[
M\left(-\frac{1}{\tau}|\frac{z}{\tau}\right)=\left(\tau\right)^{4a+6b-2\alpha}e^{\frac{2\pi i\left(\alpha+\beta\right)z^{2}}{\tau}}M\left(\tau,z\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Warmup
\end_layout

\begin_layout Standard
Start with reconstructing 
\begin_inset Formula $\eta$
\end_inset

 and 
\begin_inset Formula $\theta_{1}$
\end_inset

.
 For 
\begin_inset Formula $\eta$
\end_inset


\begin_inset Formula 
\[
\eta\left(\tau+1\right)=e^{\pi i/12}\eta\left(\tau\right)
\]

\end_inset


\begin_inset Formula 
\[
\eta\left(-\frac{1}{\tau}\right)=\sqrt{-i\tau}\eta\left(\tau\right)
\]

\end_inset

We need to take it to the power 24 because it should be invariant under 
\begin_inset Formula $\tau\to\tau+1$
\end_inset

,
 thus 
\begin_inset Formula $\Delta$
\end_inset


\begin_inset Formula 
\[
\Delta\left(\tau\right)=\left(\eta\left(\tau\right)\right)^{24}
\]

\end_inset

is modular.
 Now
\begin_inset Formula 
\[
\Delta\left(\tau+1\right)=\Delta\left(\tau\right)
\]

\end_inset


\begin_inset Formula 
\[
\Delta\left(-\frac{1}{\tau}\right)=\left(\sqrt{-i\tau}\right)^{24}\Delta\left(\tau\right)=\tau^{12}\Delta\left(\tau\right)
\]

\end_inset

so 
\begin_inset Formula $k=12$
\end_inset

 and 
\begin_inset Formula $m=0$
\end_inset

 (because there is no extra phase).
 So
\begin_inset Formula 
\[
12=k=4a+6b-2\alpha
\]

\end_inset


\begin_inset Formula 
\[
0=m=\alpha+\beta\underset{\alpha,\beta\text{ non-negative}}{\underbrace{\Rightarrow}}\alpha,\beta=0\underset{\text{weight equation}}{\underbrace{\Rightarrow}}6=2a+3b\Rightarrow a=3,b=0\lor a=0,b=2
\]

\end_inset

This gives for now
\begin_inset Formula 
\[
\Delta\left(\tau\right)=C_{1}\left(E_{6}\left(\tau\right)\right)^{2}+C_{2}\left(E_{4}\left(\tau\right)\right)^{3}
\]

\end_inset

Now we do 
\begin_inset Formula $q$
\end_inset

-expansion.
 
\begin_inset Formula 
\[
q+O\left(q^{2}\right)=\Delta\left(\tau\right)=C_{1}\left(E_{6}\left(\tau\right)\right)^{2}+C_{2}\left(E_{4}\left(\tau\right)\right)^{3}=C_{1}+C_{2}+\left(720C_{2}-1008C_{1}\right)q+O\left(q^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow-C_{1}=C_{2}=1/1728
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow\Delta\left(\tau\right)=\frac{\left(E_{4}\left(\tau\right)\right)^{3}-\left(E_{6}\left(\tau\right)\right)^{2}}{1728}
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow\eta\left(\tau\right)=e^{\frac{2\pi il}{24}}\left(\frac{\left(E_{4}\left(\tau\right)\right)^{3}-\left(E_{6}\left(\tau\right)\right)^{2}}{1728}\right)^{1/24}
\]

\end_inset

where we also need to figure out the branch 
\begin_inset Formula $l=1,...,23$
\end_inset

.
 
\end_layout

\begin_layout Standard
For 
\begin_inset Formula $\theta_{1}$
\end_inset


\begin_inset Formula 
\[
\theta_{1}\left(\tau+1|z\right)=e^{\pi i/4}\theta_{1}\left(\tau,z\right)
\]

\end_inset


\begin_inset Formula 
\[
\theta_{1}\left(-\frac{1}{\tau}|\frac{z}{\tau}\right)=-i\sqrt{-i\tau}e^{\frac{\pi iz^{2}}{\tau}}\theta_{1}\left(\tau|z\right)
\]

\end_inset

need to take power of 8
\begin_inset Formula 
\[
\Theta_{1}\left(\tau,z\right)=\left(\theta_{1}\left(\tau,z\right)\right)^{8}
\]

\end_inset

which gives
\begin_inset Formula 
\[
\Theta_{1}\left(\tau+1|z\right)=\Theta_{1}\left(\tau,z\right)
\]

\end_inset


\begin_inset Formula 
\[
\Theta_{1}\left(-\frac{1}{\tau}|\frac{z}{\tau}\right)=\tau^{4}e^{\frac{8\pi iz^{2}}{\tau}}\theta_{1}\left(\tau|z\right)
\]

\end_inset

thus
\begin_inset Formula 
\[
4=k=4a+6b-2\alpha
\]

\end_inset


\begin_inset Formula 
\[
4=m=\alpha+\beta
\]

\end_inset

The set of solutions give
\begin_inset Formula 
\begin{align*}
\Theta_{1}\left(\tau,z\right) & =C_{1}E_{4}\left(\tau\right)\phi_{0,1}^{4}\left(\tau,z\right)+C_{2}E_{6}\left(\tau\right)\phi_{-2,1}\left(\tau,z\right)\phi_{0,1}^{3}\left(\tau,z\right)+C_{3}E_{4}^{2}\left(\tau\right)\phi_{-2,1}^{2}\left(\tau,z\right)\phi_{0,1}^{2}\left(\tau,z\right)\\
 & +C_{4}E_{4}\left(\tau\right)E_{6}\left(\tau\right)\phi_{-2,1}^{3}\left(\tau,z\right)\phi_{0,1}\left(\tau,z\right)+C_{5}E_{6}^{2}\left(\tau\right)\phi_{-2,1}^{4}\left(\tau,z\right)+C_{6}E_{4}^{3}\left(\tau\right)\phi_{-2,1}^{4}\left(\tau,z\right)
\end{align*}

\end_inset

match with
\begin_inset Formula 
\[
M\left(-\frac{1}{\tau}|\frac{z}{\tau}\right)=\left(\tau\right)^{4a+6b-2\alpha}e^{\frac{2\pi i\left(\alpha+\beta\right)z^{2}}{\tau}}M\left(\tau,z\right)
\]

\end_inset

Upon equating up to order 
\begin_inset Formula $O\left(q^{0}\right)$
\end_inset

,
 we get 
\begin_inset Formula $C_{1,...,4}=0$
\end_inset

 and 
\begin_inset Formula $C_{5}=-C_{6}$
\end_inset

.
 The order 
\begin_inset Formula $O\left(q^{1}\right)$
\end_inset

 gives:
\begin_inset Formula 
\[
C_{6}=\frac{1}{1728}
\]

\end_inset

Therefore,
\begin_inset Formula 
\begin{align*}
\Theta_{1}\left(\tau,z\right) & =\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728}\phi_{-2,1}^{4}\left(\tau,z\right)\\
 & =\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728}\left(\frac{\theta_{1}\left(z,q\right)^{2}}{\eta\left(q\right)^{6}}\right)^{4}\\
 & =\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728\eta^{24}\left(q\right)}\theta_{1}\left(z,q\right)^{8}\\
\theta_{1}\left(\tau,z\right) & =e^{\frac{2\pi il}{8}}\frac{\left(E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)\right)^{\frac{1}{8}}}{\eta\left(q\right)^{\frac{6}{8}}\left(1728\right)^{\frac{1}{8}}}\theta_{1}\left(z,q\right)
\end{align*}

\end_inset

So,
 if what we did is true,
 we should get:
\begin_inset Formula 
\[
\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728\eta^{24}\left(q\right)}=1
\]

\end_inset


\end_layout

\begin_layout Subsection
Getting rank 1 from modular properties
\end_layout

\begin_layout Standard
We want to recover as a warmup the partition function for the rank 1 case:
\begin_inset Formula 
\[
Z_{n}=\frac{2\left\lfloor \frac{n}{2}\right\rfloor +1}{\left(2\pi\right)^{2}}\frac{\theta_{1}\left(y\right)}{\eta\left(q\right)}\left(\frac{\theta_{1}\left(y^{2}\right)}{\eta\left(q\right)}\right)^{2n}
\]

\end_inset


\end_layout

\begin_layout Subsubsection
The 
\begin_inset Formula $n=1$
\end_inset

 case
\end_layout

\begin_layout Standard
Let's start from 
\begin_inset Formula $n=1$
\end_inset

.
 The modular properties are
\begin_inset Formula 
\[
Z_{1}\left(\tau+1|u\right)=e^{i\frac{\pi}{2}}Z_{n}\left(\tau|u\right)
\]

\end_inset


\begin_inset Formula 
\[
Z_{1}\left(-\frac{1}{\tau}|\frac{u}{\tau}\right)=ie^{\frac{\pi i36u^{2}}{\tau}}Z_{n}\left(\tau|u\right)
\]

\end_inset

Let's define:
\begin_inset Formula 
\[
Y=Z_{1}^{4}
\]

\end_inset

Therefore,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(C\tau+D\right)^{k}e^{\frac{2\pi imCz^{2}}{C\tau+D}}\phi\left(\tau,z\right)
\]

\end_inset

Thus,
 the weight 
\begin_inset Formula $k$
\end_inset

 and index 
\begin_inset Formula $m$
\end_inset

 are:
\begin_inset Formula 
\begin{align*}
k & =0\\
m & =18
\end{align*}

\end_inset

Applying the algorithm for the reconstruction of a function 
\begin_inset Formula 
\begin{align*}
Y & =\frac{\text{\ensuremath{\phi}2}^{6}\left(\text{E4}^{3}-\text{E6}^{2}\right)\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}+\text{E6}\text{\ensuremath{\phi}2}^{3}-864\text{\ensuremath{\phi}0}^{3}\right)^{4}}{3761479876608}\\
 & =\eta^{24}\left(q\right)\frac{\text{\ensuremath{\phi}2}^{6}\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}+\text{E6}\text{\ensuremath{\phi}2}^{3}-864\text{\ensuremath{\phi}0}^{3}\right)^{4}}{2176782336}\\
 & =\eta^{24}\left(q\right)\frac{\text{\ensuremath{\phi}2}^{6}\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}+\text{E6}\text{\ensuremath{\phi}2}^{3}-864\text{\ensuremath{\phi}0}^{3}\right)^{4}}{2176782336}
\end{align*}

\end_inset

On the other hand,
 we have:
\begin_inset Formula 
\begin{align*}
\theta_{1}\left(z^{2}\right)^{8} & =\frac{\left(\text{E4}^{3}-\text{E6}^{2}\right)\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\text{\ensuremath{\phi}2}\right)^{4}}{3761479876608}\\
 & =\eta^{24}\frac{\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\text{\ensuremath{\phi}2}\right)^{4}}{2176782336}\\
\implies\theta_{1}\left(z^{2}\right)^{2} & =\eta^{6}\frac{18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\text{\ensuremath{\phi}2}}{216}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\eta^{24}\left(q\right)=\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\left(\theta_{1}\left(\tau,z\right)\right)^{8} & =\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728}\phi_{-2,1}^{4}\left(\tau,z\right)
\end{align*}

\end_inset

Therefore,
\begin_inset Formula 
\begin{align*}
Y & =\text{\ensuremath{\phi}2}^{2}\theta_{1}\left(z^{2}\right)^{8}\\
 & =\frac{\theta_{1}^{4}}{\eta^{12}}\theta_{1}\left(z^{2}\right)^{8}
\end{align*}

\end_inset

Whence,
\begin_inset Formula 
\[
Z_{1}=\frac{\theta_{1}\left(z\right)\theta_{1}\left(z^{2}\right)^{2}}{\eta^{3}}
\]

\end_inset

We can compare this result to the exact form:
\begin_inset Formula 
\[
Z_{1}=\frac{\theta_{1}\left(y\right)}{\eta\left(q\right)}\left(\frac{\theta_{1}\left(y^{2}\right)}{\eta\left(q\right)}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Section
Getting rank 0 from modular properties
\end_layout

\begin_layout Subsection
For 
\begin_inset Formula $n=\left(0,0\right)$
\end_inset


\end_layout

\begin_layout Standard
We know the exact result is
\begin_inset Formula 
\[
Z_{\text{exact}}=\frac{\theta_{1}\left(fh\right)}{\theta_{1}\left(f^{2}h^{2}\right)}
\]

\end_inset

set 
\begin_inset Formula $h\to1$
\end_inset

 as we still only handle one variable.
 Need to take it to the power of 
\begin_inset Formula $-2$
\end_inset

 to get a Jacobi form
\begin_inset Formula 
\[
Z_{\text{exact}}^{-2}=\frac{\theta_{1}^{2}\left(f^{2}\right)}{\theta_{1}^{2}\left(f\right)}
\]

\end_inset

alternatively say we don't solve exactly.
 from integrand know we need the power of 
\begin_inset Formula $-2$
\end_inset

 and the modular properties (weight=0 index=3).
 q expand and perform contour integral.
 Modular reconstruct and get
\begin_inset Formula 
\begin{align*}
Z_{\text{reconst}}^{-2} & =-\frac{1}{3}\pi^{2}\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}-\frac{1}{54}\pi^{2}\text{E6}\text{\ensuremath{\phi}2}^{3}+16\pi^{2}\text{\ensuremath{\phi}0}^{3}\\
 & =\pi^{2}\left(-\frac{1}{3}\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}-\frac{1}{54}\text{E6}\text{\ensuremath{\phi}2}^{3}+16\text{\ensuremath{\phi}0}^{3}\right)\\
 & =\frac{\pi^{2}}{54}\left(-18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}-\text{E6}\text{\ensuremath{\phi}2}^{3}+864\text{\ensuremath{\phi}0}^{3}\right)\\
 & =-\frac{4\pi^{2}}{\phi_{-2,1}\eta^{6}}\eta^{6}\phi_{-2,1}\frac{18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}+\text{E6}\text{\ensuremath{\phi}2}^{3}-864\text{\ensuremath{\phi}0}^{3}}{256}\\
 & =\frac{4\pi^{2}}{\phi_{-2,1}\eta^{6}}\theta_{1}\left(z^{2}\right)^{2}\\
 & =\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
For 
\begin_inset Formula $n=\left(1,0\right)$
\end_inset


\end_layout

\begin_layout Standard
Found
\begin_inset Formula 
\begin{align*}
Z_{\left(1,0\right)}^{-2} & =-\frac{1}{3}\pi^{2}\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}-\frac{1}{54}\pi^{2}\text{E6}\text{\ensuremath{\phi}2}^{3}+16\pi^{2}\text{\ensuremath{\phi}0}^{3}
\end{align*}

\end_inset

which is exactly the 
\begin_inset Formula $n=\left(0,0\right)$
\end_inset

 case
\begin_inset Formula 
\[
Z_{\left(1,0\right)}^{-2}=\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\end_layout

\begin_layout Subsection
For 
\begin_inset Formula $n=\left(2,0\right)$
\end_inset


\end_layout

\begin_layout Standard
Found
\begin_inset Formula 
\begin{align*}
Z_{\left(2,0\right)}^{-2} & =-\frac{1}{27}\pi^{2}\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}-\frac{1}{486}\pi^{2}\text{E6}\text{\ensuremath{\phi}2}^{3}+\frac{16\pi^{2}\text{\ensuremath{\phi}0}^{3}}{9}\\
 & =\frac{1}{9}Z_{\left(1,0\right)}^{-2}\\
 & =\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{9\theta_{1}^{2}\left(z\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
For 
\begin_inset Formula $n=\left(3,0\right)$
\end_inset


\end_layout

\begin_layout Standard
Found same as 
\begin_inset Formula $n=\left(2,0\right)$
\end_inset


\begin_inset Formula 
\[
Z_{\left(3,0\right)}^{-2}=Z_{\left(2,0\right)}^{-2}=\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{9\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\end_layout

\begin_layout Subsection
For 
\begin_inset Formula $n=\left(4,0\right)$
\end_inset


\end_layout

\begin_layout Standard
Found
\begin_inset Formula 
\begin{align*}
Z_{\left(4,0\right)}^{-2} & =-\frac{1}{75}\pi^{2}\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}-\frac{\pi^{2}\text{E6}\text{\ensuremath{\phi}2}^{3}}{1350}+\frac{16\pi^{2}\text{\ensuremath{\phi}0}^{3}}{25}\\
 & =\frac{1}{25}Z_{\left(1,0\right)}^{-2}\\
 & =\frac{4\pi^{2}}{25}\frac{\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Claim for 
\begin_inset Formula $n=\left(n_{1},0\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{\left(0,0\right)}^{-2}=\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(1,0\right)}^{-2}=\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(2,0\right)}^{-2}=\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{9\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(3,0\right)}^{-2}=\frac{4\pi^{2}\theta_{1}^{2}\left(z^{2}\right)}{9\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(4,0\right)}^{-2}=\frac{4\pi^{2}}{25}\frac{\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\begin_inset Formula 
\[
Z_{\left(n_{1},0\right)}^{-2}=\frac{4\pi^{2}}{\left(2\left\lfloor \frac{n_{1}}{2}\right\rfloor +1\right)^{2}}\frac{\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}
\]

\end_inset


\end_layout

\begin_layout Subsection
Claim for 
\begin_inset Formula $n=\left(0,1\right)$
\end_inset


\end_layout

\begin_layout Standard
For 
\begin_inset Formula $n=\left(0,1\right)$
\end_inset

 the integrand is:
 
\begin_inset Formula 
\[
I_{\left(0,1\right)}=\frac{\text{\ensuremath{\eta}}^{3}\theta_{1}\left(\frac{1}{h}\right)\theta_{1}\left(\frac{f}{x}\right)\theta_{1}\left(\frac{x}{fh^{2}}\right)\theta_{1}\left(\frac{x}{fh}\right)^{2}}{\theta_{1}(x)^{2}\theta_{1}(fh)^{2}\theta_{1}\left(\frac{f^{2}h^{2}}{x}\right)^{2}}\d u
\]

\end_inset

Removing 
\begin_inset Formula $\theta_{1}\left(\frac{1}{h}\right)$
\end_inset

 and setting 
\begin_inset Formula $h\to1$
\end_inset

,
 we get:
\begin_inset Formula 
\[
\tilde{I}_{\left(0,1\right)}=\frac{\text{\ensuremath{\eta}}^{3}\theta_{1}\left(\frac{f}{x}\right)\theta_{1}\left(\frac{x}{f}\right)^{3}}{\theta_{1}(x)^{2}\theta_{1}(f)^{2}\theta_{1}\left(\frac{f^{2}}{x}\right)^{2}}\d u
\]

\end_inset

This function has weight 
\begin_inset Formula $-\frac{1}{2}$
\end_inset

 (including 
\begin_inset Formula $\d u$
\end_inset

) and index 
\begin_inset Formula $-3$
\end_inset

.
 Let's modify it to:
\begin_inset Formula 
\[
J_{\left(0,1\right)}=\left(\left(\int\tilde{I}_{\left(0,1\right)}\right)\theta_{1}\left(\frac{1}{f}\right)\theta_{1}\left(f^{2}\right)^{3}\eta^{3}\right)^{2}
\]

\end_inset


\begin_inset Formula $J_{(0,1)}$
\end_inset

 has weight 6 and index 7.
 We found after reconstruction:
\begin_inset Formula 
\begin{align*}
J_{\left(0,1\right)} & =-\frac{\text{\ensuremath{\phi}2}^{3}\left(\text{E4}^{3}-\text{E6}^{2}\right)\left(\text{E4}\text{\ensuremath{\phi}2}^{2}-144\text{\ensuremath{\phi}0}^{2}\right)^{2}}{995328}\\
 & =-\frac{1}{576}\eta^{24}\text{\ensuremath{\phi}2}^{3}\left(\text{E4}\text{\ensuremath{\phi}2}^{2}-144\text{\ensuremath{\phi}0}^{2}\right)^{2}
\end{align*}

\end_inset

We know that the final result is (through direct application of the residue formula):
\begin_inset Formula 
\[
Z_{(0,1)}=\frac{1}{2\pi^{2}\eta^{3}}\frac{\theta_{1}^{2}\left(v\right)}{\theta_{1}\left(2v\right)}\frac{\d}{\d v}\left(\frac{\theta_{1}\left(v\right)}{\theta_{1}\left(2v\right)}\right)
\]

\end_inset

where 
\begin_inset Formula $f=e^{2\pi iv}$
\end_inset

.
\end_layout

\begin_layout Section
Getting rank 2 from modular properties
\end_layout

\begin_layout Subsection
Modular transformation
\end_layout

\begin_layout Standard
The partition function transforms as:
\begin_inset Formula 
\begin{align*}
Z_{n} & \overset{T}{\to}e^{\frac{i\pi}{2}\left(n-1\right)}Z_{n} & Z_{n} & \overset{S}{\to}\left(i\right)^{n-1}\tau^{n-3}e^{\frac{i\pi}{\tau}14n}Z_{n}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Case 
\begin_inset Formula $n=-1$
\end_inset


\end_layout

\begin_layout Standard
The integrand is:
\begin_inset Formula 
\[
I_{-1}=-\frac{\theta_{1}\left(\frac{1}{w_{1}y}\right)\theta_{1}\left(w_{1}y\right)\theta_{1}\left(\frac{1}{w_{2}y}\right)\theta_{1}\left(w_{2}y\right)}{\theta_{1}\left(w_{1}\right)\theta_{1}\left(w_{2}\right)\theta_{1}\left(\frac{1}{y^{2}}\right)^{2}\theta_{1}\left(\frac{1}{y}\right)^{2}\theta_{1}\left(\frac{1}{w_{1}y^{2}}\right)\theta_{1}\left(\frac{1}{w_{2}y^{2}}\right)}\d u_{1}\d u_{2}
\]

\end_inset

Its modular transformation is:
\begin_inset Formula 
\begin{align*}
I_{-1} & \overset{T}{\to}-I_{-1} & I_{-1} & \overset{S}{\to}-\frac{1}{\tau^{4}}e^{-\frac{14i\pi v^{2}}{\tau}}I_{-1}
\end{align*}

\end_inset

The 
\begin_inset Formula $S$
\end_inset

-duality takes into account an additional factor 
\begin_inset Formula $\tau^{-2}$
\end_inset

 coming from the integration measure 
\begin_inset Formula $\d u_{1}\d u_{2}$
\end_inset

.
 In this case we can compute the integral exactly and get:
\begin_inset Formula 
\[
Z_{-1}=\int_{JK}I_{-1}=\frac{\theta_{1}(y)^{2}}{\eta^{6}\left(q\right)\theta_{1}\left(\frac{1}{y^{2}}\right)^{4}}
\]

\end_inset

This function has the same modular properties.
\end_layout

\begin_layout Standard
We'll now attempt to reconstruct this function from only the modular properties and a q-expansion of the integral.
 We reconstruct the following Jacobi modular form:
\begin_inset Formula 
\[
J_{-1}=\left(Z_{-1}\right)^{-2}
\]

\end_inset


\begin_inset Formula $J_{-1}$
\end_inset

 has weight 
\begin_inset Formula $8$
\end_inset

 and index 
\begin_inset Formula $14$
\end_inset

.
 Applying the modular reconstruction algorithm,
 we get:
\begin_inset Formula 
\begin{align*}
J_{-1} & =\frac{\text{\ensuremath{\phi}2}^{2}\left(\text{E4}^{3}-\text{E6}^{2}\right)\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{2}+\text{E6}\text{\ensuremath{\phi}2}^{3}-864\text{\ensuremath{\phi}0}^{3}\right)^{4}}{3761479876608}\\
 & =\frac{\text{\ensuremath{\phi}2}^{-2}\left(\text{E4}^{3}-\text{E6}^{2}\right)\left(18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\phi2\right)^{4}}{3761479876608}\\
\left\{ \text{Appendix}\right\} \to & =\theta_{1}\left(z^{2}\right)^{8}\text{\ensuremath{\phi}2}^{-2}\\
 & =\theta_{1}\left(z^{2}\right)^{8}\left(\frac{\theta_{1}\left(z\right)^{2}}{\eta^{6}\left(q\right)}\right)^{-2}\\
 & =\left(\frac{\theta_{1}\left(z\right)^{2}}{\eta^{6}\left(q\right)\theta_{1}\left(z^{2}\right)^{4}}\right)^{-2}
\end{align*}

\end_inset

Hence,
\begin_inset Formula 
\[
Z_{-1}^{\text{reconstructed}}=\frac{\theta_{1}\left(z\right)^{2}}{\eta^{6}\left(q\right)\theta_{1}\left(z^{2}\right)^{4}}
\]

\end_inset

as expected.
\end_layout

\begin_layout Subsection
Case 
\begin_inset Formula $n=1$
\end_inset


\end_layout

\begin_layout Standard
In that case we get the following modular transformation:
\begin_inset Formula 
\begin{align*}
I_{1} & \overset{T}{\to}I_{1} & I_{1} & \overset{S}{\to}\frac{1}{\tau^{2}}e^{\frac{14i\pi v^{2}}{\tau}}I_{1}
\end{align*}

\end_inset

We'll reconstruct the function:
\begin_inset Formula 
\[
J_{1}=I_{1}\phi_{-2,1}^{4}
\]

\end_inset

 which have weight 
\begin_inset Formula $2$
\end_inset

 and index 
\begin_inset Formula $11$
\end_inset

.
\end_layout

\begin_layout Section
Getting rank 3 from modular properties
\end_layout

\begin_layout Subsection
Modular transformation
\end_layout

\begin_layout Standard
The partition function transforms as:
\begin_inset Formula 
\begin{align*}
Z_{n} & \overset{T}{\to}e^{\frac{3i\pi}{4}\left(n-1\right)}Z_{n} & Z_{n} & \overset{S}{\to}e^{\frac{i\pi}{4}\left(n-1\right)}\tau^{\frac{1}{2}\left(3n-7\right)}e^{\frac{i\pi}{\tau}21n}Z_{n}
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Summary of modular properties
\end_layout

\begin_layout Standard
For 
\begin_inset Formula $r=1$
\end_inset

,
\begin_inset Formula 
\begin{align*}
Z_{n} & \overset{T}{\to}e^{\frac{i\pi}{6}\left(2n+1\right)}Z_{n} & Z_{n} & \overset{S}{\to}\left(-i\right)^{2n+1}e^{\frac{\pi i\left(8n+1\right)u^{2}}{\tau}}Z_{n}
\end{align*}

\end_inset

For 
\begin_inset Formula $r=2$
\end_inset

,
\begin_inset Formula 
\begin{align*}
Z_{n} & \overset{T}{\to}e^{\frac{i\pi}{2}\left(n-1\right)}Z_{n} & Z_{n} & \overset{S}{\to}e^{\frac{i\pi}{2}\left(n-1\right)}\tau^{n-3}e^{\frac{i\pi}{\tau}14nu^{2}}Z_{n}
\end{align*}

\end_inset

For 
\begin_inset Formula $r=3$
\end_inset

,
\begin_inset Formula 
\begin{align*}
Z_{n} & \overset{T}{\to}e^{\frac{3i\pi}{4}\left(n-1\right)}Z_{n} & Z_{n} & \overset{S}{\to}e^{\frac{i\pi}{4}\left(n-1\right)}\tau^{\frac{1}{2}\left(3n-7\right)}e^{\frac{i\pi}{\tau}21nu^{2}}Z_{n}
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Appendix - Summary of modular reconstructions
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\eta^{24}\left(q\right)=\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728}
\]

\end_inset


\begin_inset Formula 
\[
\left(\theta_{1}\left(\tau,z\right)\right)^{8}=\frac{E_{4}^{3}\left(\tau\right)-E_{6}^{2}\left(\tau\right)}{1728}\phi_{-2,1}^{4}\left(\tau,z\right)=\eta^{24}\left(q\right)\phi_{-2,1}^{4}\left(\tau,z\right)\Rightarrow\theta_{1}^{2}\left(\tau,z\right)=\eta^{6}\left(q\right)\phi_{-2,1}\left(\tau,z\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\theta_{1}\left(z^{2}\right)^{8} & =\frac{\left(E_{4}^{3}-E_{6}^{2}\right)\left(18E_{4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\text{\ensuremath{\phi}2}\right)^{4}}{3761479876608}\\
 & =\eta^{24}\frac{\left(18E_{4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\text{\ensuremath{\phi}2}\right)^{4}}{2176782336}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
 & \Rightarrow\theta_{1}\left(z^{2}\right)^{2}=\underset{\text{found by checking q expansion}}{\underbrace{-1}}\eta^{6}\frac{18\text{E4}\text{\ensuremath{\phi}0}\text{\ensuremath{\phi}2}^{3}+\text{E6}\text{\ensuremath{\phi}2}^{4}-864\text{\ensuremath{\phi}0}^{3}\text{\ensuremath{\phi}2}}{216}\\
 & \Rightarrow\theta_{1}\left(z^{2}\right)^{2}=-\eta^{6}\phi_{-2,1}\frac{18E_{4}\phi_{0,1}\phi_{-2,1}^{2}+E_{6}\phi_{-2,1}^{3}-864\phi_{0,1}^{3}}{216}\\
 & \Rightarrow\theta_{1}\left(z^{2}\right)^{2}=-\theta_{1}^{2}\left(\tau,z\right)\frac{18E_{4}\phi_{0,1}\phi_{-2,1}^{2}+E_{6}\phi_{-2,1}^{3}-864\phi_{0,1}^{3}}{216}\\
 & \Rightarrow\frac{\theta_{1}^{2}\left(z^{2}\right)}{\theta_{1}^{2}\left(z\right)}=\frac{864\phi_{0,1}^{3}-18E_{4}\phi_{0,1}\phi_{-2,1}^{2}-E_{6}\phi_{-2,1}^{3}}{216}
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Modular properties of basic building blocks
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\eta\left(\tau+1\right)=e^{\pi i/12}\eta\left(\tau\right)
\]

\end_inset


\begin_inset Formula 
\[
\eta\left(-\frac{1}{\tau}\right)=\sqrt{-i\tau}\eta\left(\tau\right)
\]

\end_inset

or 
\begin_inset Formula $\Delta=\eta^{24}$
\end_inset


\begin_inset Formula 
\[
\Delta\left(\tau+1\right)=\Delta\left(\tau\right)
\]

\end_inset


\begin_inset Formula 
\[
\Delta\left(-\frac{1}{\tau}\right)=\tau^{12}\Delta\left(\tau\right)
\]

\end_inset

which means weight 
\begin_inset Formula $k=12$
\end_inset

 and index 
\begin_inset Formula $m=0$
\end_inset

 (obviously,
 as no 
\begin_inset Formula $z$
\end_inset

 dependence).
\begin_inset Formula 
\[
\theta_{1}\left(\tau+1|z\right)=e^{\pi i/4}\theta_{1}\left(\tau,z\right)
\]

\end_inset


\begin_inset Formula 
\[
\theta_{1}\left(-\frac{1}{\tau}|\frac{z}{\tau}\right)=-i\sqrt{-i\tau}e^{\frac{\pi iz^{2}}{\tau}}\theta_{1}\left(\tau|z\right)
\]

\end_inset


\begin_inset Formula 
\[
\theta_{1}\left(\tau|z+a+b\tau\right)=\left(-1\right)^{a+b}e^{-2\pi ibz-i\pi b^{2}\tau}\theta_{1}\left(\tau|z\right)
\]

\end_inset

or 
\begin_inset Formula $\Theta_{1}=\theta_{1}^{8}$
\end_inset


\begin_inset Formula 
\[
\Theta_{1}\left(\tau+1|z\right)=\Theta_{1}\left(\tau,z\right)
\]

\end_inset


\begin_inset Formula 
\[
\Theta_{1}\left(-\frac{1}{\tau}|\frac{z}{\tau}\right)=\tau^{4}e^{\frac{8\pi iz^{2}}{\tau}}\Theta_{1}\left(\tau|z\right)
\]

\end_inset


\begin_inset Formula 
\[
\Theta_{1}\left(\tau|z+a+b\tau\right)=e^{-2\pi i\cdot4\left(2bz+b^{2}\tau\right)}\Theta_{1}\left(\tau|z\right)
\]

\end_inset

which means weight 
\begin_inset Formula $k=4$
\end_inset

 and index 
\begin_inset Formula $m=4$
\end_inset

.
 
\end_layout

