\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Table of charges
\end_layout

\end_inset


\end_layout

\end_inset

The superpotential is
\begin_inset Formula 
\[
W=\varphi_{1}Q_{1}\varphi_{2}+\varphi_{2}Q_{2}\varphi_{1}+q_{1}Q_{1}^{2}+q_{2}Q_{2}^{2}
\]

\end_inset


\begin_inset Formula 
\[
f_{\chi}\left(\vec{g},a,r\right)=\left(\frac{1}{\theta_{1}\left(x^{g}y^{a};q\right)}\right)^{\vec{g}\cdot\vec{m}+an-r+1}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\mathcal{I}\left(y,n\right) & =\frac{1}{4}\sum_{\vec{m}\in\mathbb{Z}}\frac{1}{\left(2\pi i\right)^{2}}\int\left(i\right)^{2}\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \prod_{g_{1},g_{2}=2,0,-2}f_{\chi}\left(\left(g_{1},0\right),-1,1\right)f_{\chi}\left(\left(0,g_{2}\right),-1,1\right)\prod_{g_{1},g_{2}=\pm1}\left(f_{\chi}\left(\left(g_{1},g_{2}\right),1,0\right)\right)^{2}\\
 & =\frac{1}{4}\sum_{\vec{m}\in\mathbb{Z}}\frac{1}{\left(2\pi\right)^{2}}\int\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}y^{-1};q\right)}\right)^{g_{1}m_{1}-n}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{2}}y^{-1};q\right)}\right)^{g_{2}m_{2}-n}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}x_{2}^{g_{2}}y;q\right)}\right)^{2\left(g_{1}m_{1}+g_{2}m_{2}+n+1\right)}m_{1}<m_{2}<0\\
 & =\frac{1}{4}\sum_{i=1}^{8}\sum_{\vec{m}\in D_{i}}\sum_{\vec{u}\in U^{*}}I\left(m_{1},m_{2},u_{1}^{*},u_{2}^{*}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\[
D_{2}=\left\{ \left(m_{1},-m_{2}\right):\left(m_{1},m_{2}\right)\in D_{1}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
D_{3}=\left\{ \left(m_{2},m_{1}\right):\left(m_{1},m_{2}\right)\in D_{1}\right\} 
\]

\end_inset


\begin_inset Formula 
\begin{align*}
I\left(m_{1},m_{2},u_{1}^{*},u_{2}^{*}\right) & =\frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}y^{-1};q\right)}\right)^{g_{1}m_{1}-n}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{2}}y^{-1};q\right)}\right)^{g_{2}m_{2}-n}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}x_{2}^{g_{2}}y;q\right)}\right)^{2\left(g_{1}m_{1}+g_{2}m_{2}+n+1\right)}\\
I\left(m_{2},m_{1},u_{1}^{*},u_{2}^{*}\right) & =\frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}y^{-1};q\right)}\right)^{g_{1}m_{2}-n}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{2}}y^{-1};q\right)}\right)^{g_{2}m_{1}-n}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}x_{2}^{g_{2}}y;q\right)}\right)^{2\left(g_{1}m_{2}+g_{2}m_{1}+n+1\right)}\\
 & =\frac{1}{2}\frac{1}{2\pi i}\int_{u_{1}=u_{1}^{*},u_{2}=u_{2}^{*}}i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{2}}y^{-1};q\right)}\right)^{g_{2}m_{2}-n}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{1}}y^{-1};q\right)}\right)^{g_{1}m_{1}-n}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{2}}x_{2}^{g_{1}}y;q\right)}\right)^{2\left(g_{2}m_{2}+g_{1}m_{1}+n+1\right)}\\
 & =\frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{2}}y^{-1};q\right)}\right)^{g_{2}m_{2}-n}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{1}}y^{-1};q\right)}\right)^{g_{1}m_{1}-n}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{2}}x_{2}^{g_{1}}y;q\right)}\right)^{2\left(g_{2}m_{2}+g_{1}m_{1}+n+1\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection*
Jeffrey-Kirwan Residue
\end_layout

\begin_layout Standard
We write the covectors with 
\begin_inset Formula $Q_{x,y}=\left(x,y\right)$
\end_inset

,
 e.g.
 
\begin_inset Formula $Q_{2,0}$
\end_inset

 is the covector 
\begin_inset Formula $\left(2,0\right)$
\end_inset

.
 We have
\begin_inset Formula 
\[
\left(2,0\right),\left(0,2\right),\left(-2,0\right),\left(0,-2\right),\left(-1,1\right)\left(-1,-1\right),\left(1,-1\right)\left(1,1\right)
\]

\end_inset

Similarly 
\begin_inset Formula $H_{x,y}$
\end_inset

 is the hyperplane associated to the covector 
\begin_inset Formula $Q_{x,y}$
\end_inset

,
 where if several hyperplanes are relevant we use 
\begin_inset Formula $H_{x,y}^{a,b}$
\end_inset

 where 
\begin_inset Formula $a,b$
\end_inset

 are the integers showing up in 
\begin_inset Formula $a+b\tau$
\end_inset

.
 Each hypeplane is actually a singularity only if the power of the function is positive.
\begin_inset Formula 
\begin{align*}
 & H_{2,0}^{a,b}: &  & 2\left(u_{1}+\frac{1}{2}\left(a+b\tau\right)\right)-v=0\mod{a+b\tau}\\
 & \text{condition:} &  & 2m_{1}-n>0\\
 & H_{-2,0}^{a,b}: &  & -2\left(u_{1}+\frac{1}{2}\left(a+b\tau\right)\right)-v=0\mod{a+b\tau}\\
 & \text{condition:} &  & -2m_{1}-n>0\\
 & H_{0,2}^{a,b}: &  & 2\left(u_{2}+\frac{1}{2}\left(a+b\tau\right)\right)-v=0\mod{a+b\tau}\\
 & \text{condition:} &  & 2m_{2}-n>0\\
 & H_{0,-2}^{a,b}: &  & -2\left(u_{2}+\frac{1}{2}\left(a+b\tau\right)\right)-v=0\mod{a+b\tau}\\
 & \text{condition:} &  & -2m_{2}-n>0\\
 & H_{1,1}: &  & u_{1}+u_{2}+v=0\\
 & \text{condition:} &  & m_{1}+m_{2}+n+1>0\\
 & H_{1,-1}: &  & u_{1}-u_{2}+v=0\\
 & \text{condition:} &  & m_{1}-m_{2}+n+1>0\\
 & H_{-1,1}: &  & -u_{1}+u_{2}+v=0\\
 & \text{condition:} &  & -m_{1}+m_{2}+n+1>0\\
 & H_{-1,-1}: &  & -u_{1}-u_{2}+v=0\\
 & \text{condition:} &  & -m_{1}-m_{2}+n+1>0
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection*
Region 1
\end_layout

\begin_layout Standard
For 
\begin_inset Formula $m_{1}\leq m_{2}<0$
\end_inset

 choose 
\begin_inset Formula $\vec{\eta}=-\vec{m}$
\end_inset

 which promises truncation of sum.
 All the cones that 
\begin_inset Formula $\eta$
\end_inset

 sits in are 
\begin_inset Formula 
\[
\text{Cone}\left(Q_{2,0},Q_{1,1}\right),\text{Cone}\left(Q_{2,0},Q_{0,2}\right),\text{Cone}\left(Q_{2,0},Q_{-1,1}\right),\text{Cone}\left(Q_{1,-1},Q_{1,1}\right),\text{Cone}\left(Q_{1,-1},Q_{0,2}\right),\text{Cone}\left(Q_{0,-2},Q_{1,1}\right).
\]

\end_inset


\end_layout

\begin_layout Enumerate
For 
\begin_inset Formula $\text{Cone}\left(Q_{2,0},Q_{1,1}\right)$
\end_inset

 we have 
\begin_inset Formula $H_{2,0}^{a,b}\cap H_{1,1}$
\end_inset

 which gives 4 points 
\begin_inset Formula $u_{1}=\frac{1}{2}\left(v-a-b\tau\right)$
\end_inset

,
 
\begin_inset Formula $u_{2}=-u_{1}-v=-\frac{3}{2}v+\frac{1}{2}\left(a+b\tau\right)$
\end_inset

 for 
\begin_inset Formula $a,b=0,1$
\end_inset

.
 
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Enumerate
For 
\begin_inset Formula $\text{Cone}\left(Q_{2,0},Q_{0,2}\right)$
\end_inset

 we have 
\begin_inset Formula $H_{2,0}^{a,b}\cap H_{0,2}^{a,b}$
\end_inset

 which gives 16 points 
\begin_inset Formula $u_{1}=\frac{1}{2}\left(v-a-b\tau\right)$
\end_inset

,
 
\begin_inset Formula $u_{2}=\frac{1}{2}\left(v-c-d\tau\right)$
\end_inset

 for 
\begin_inset Formula $a,b,c,d=0,1$
\end_inset

.
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Enumerate
For 
\begin_inset Formula $\text{Cone}\left(Q_{2,0},Q_{-1,1}\right)$
\end_inset

 we have 
\begin_inset Formula $H_{2,0}^{a,b}\cap H_{-1,1}$
\end_inset

 which gives 4 points 
\begin_inset Formula $u_{1}=\frac{1}{2}\left(v-a-b\tau\right)$
\end_inset

,
 
\begin_inset Formula $u_{2}=u_{1}-v=-\frac{1}{2}v-\frac{1}{2}\left(a+b\tau\right)$
\end_inset

 for 
\begin_inset Formula $a,b=0,1$
\end_inset

.
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Enumerate
For 
\begin_inset Formula $\text{Cone}\left(Q_{1,-1},Q_{1,1}\right)$
\end_inset

 we have 
\begin_inset Formula $H_{1,-1}\cap H_{1,1}$
\end_inset

 which gives 1 point 
\begin_inset Formula $\left(-v,0\right)$
\end_inset

 because 
\begin_inset Formula $u_{1}=u_{2}-v$
\end_inset

,
 
\begin_inset Formula $u_{2}=-u_{1}-v=-u_{2}\Rightarrow u_{2}=0$
\end_inset

.
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Enumerate
For 
\begin_inset Formula $\text{Cone}\left(Q_{1,-1},Q_{0,2}\right)$
\end_inset

 we have 
\begin_inset Formula $H_{1,-1}\cap H_{0,2}^{a,b}$
\end_inset

 which gives 4 points 
\begin_inset Formula $u_{2}=\frac{1}{2}\left(v-a-b\tau\right)$
\end_inset

,
 
\begin_inset Formula $u_{1}=u_{2}-v=-\frac{1}{2}\left(v+a+b\tau\right)$
\end_inset

 for 
\begin_inset Formula $a,b=0,1$
\end_inset

.
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Enumerate
For 
\begin_inset Formula $\text{Cone}\left(Q_{0,-2},Q_{1,1}\right)$
\end_inset

 we have 
\begin_inset Formula $H_{0,-2}^{a,b}\cap H_{1,1}$
\end_inset

 which gives 4 points 
\begin_inset Formula $u_{2}=-\frac{1}{2}\left(v+a+b\tau\right)$
\end_inset

,
 
\begin_inset Formula $u_{1}=-u_{2}-v=-\frac{1}{2}v+\frac{1}{2}\left(a+b\tau\right)$
\end_inset

 for 
\begin_inset Formula $a,b=0,1$
\end_inset

.
\end_layout

\begin_layout Standard
For general 
\begin_inset Formula $v$
\end_inset

 there is no degeneracy - only 2 hyperplanes intersect at each point.
 This means there are only two flags for each point,
 and only one of them is going to be relevant as the subchambers will cut the chamber exactly in the middle.
 The range of 
\begin_inset Formula $\vec{m}$
\end_inset

 for each cone (limiting to 
\begin_inset Formula $m_{1}\leq m_{2}<0$
\end_inset

)
\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{1}-n>0$
\end_inset

,
 
\begin_inset Formula $m_{1}+m_{2}+n+1>0$
\end_inset

 give 
\begin_inset Formula $m_{1}>\frac{1}{2}n$
\end_inset

,
 
\begin_inset Formula $m_{2}>-m_{1}-n-1>-3m_{1}-1\underset{-m>0}{\underbrace{>}}-1$
\end_inset

.
 This condition is 
\series bold
never satisfied
\series default
.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{1}-n>0$
\end_inset

,
 
\begin_inset Formula $2m_{2}-n>0$
\end_inset

 give 
\begin_inset Formula $m_{1},m_{2}>\frac{1}{2}n$
\end_inset

.
 This will contribute for 
\begin_inset Formula $n<0$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{1}-n>0$
\end_inset

,
 
\begin_inset Formula $-m_{1}+m_{2}+n+1>0$
\end_inset

 give 
\begin_inset Formula $m_{1}>\frac{1}{2}n$
\end_inset

 which means 
\begin_inset Formula $n$
\end_inset

 must be negative and 
\begin_inset Formula $m_{2}>m_{1}-n-1>-\frac{1}{2}n-1\underset{-n\geq1}{\underbrace{\geq}}-\frac{1}{2}$
\end_inset

 which is 
\series bold
never satisfied
\series default
.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $m_{1}-m_{2}+n+1>0$
\end_inset

,
 
\begin_inset Formula $m_{1}+m_{2}+n+1>0$
\end_inset

,
 use Mathematica to find the points.
 This will contribute for 
\begin_inset Formula $n>0$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $m_{1}-m_{2}+n+1>0$
\end_inset

,
 
\begin_inset Formula $2m_{2}-n>0$
\end_inset

.
 Get 
\begin_inset Formula $m_{2}>\frac{1}{2}n$
\end_inset

 
\begin_inset Formula $\Rightarrow$
\end_inset

 
\begin_inset Formula $-m_{2}<-\frac{1}{2}n$
\end_inset

 so 
\begin_inset Formula $m_{2}+\frac{1}{2}n+1\geq m_{1}+\frac{1}{2}n+1>m_{1}-m_{2}+n+1>0$
\end_inset

 bla bla 
\series bold
can't satisfy
\series default
.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $-2m_{2}-n>0$
\end_inset

,
 
\begin_inset Formula $m_{1}+m_{2}+n+1>0$
\end_inset

.
 Get 
\begin_inset Formula $m_{1}\leq m_{2}<-\frac{1}{2}n$
\end_inset

 but 
\begin_inset Formula $m_{1}>-m_{2}-n-1>-\frac{1}{2}n-1$
\end_inset

 so 
\begin_inset Formula $-\frac{1}{2}n-1<m_{1}<-\frac{1}{2}n$
\end_inset

 which 
\series bold
can't be satisfied
\series default
.
\end_layout

\begin_layout Standard
Now for the singular case of 
\begin_inset Formula $m_{1}<0$
\end_inset

 and 
\begin_inset Formula $m_{2}=0$
\end_inset

:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{1}-n>0$
\end_inset

,
 
\begin_inset Formula $m_{1}+n+1>0$
\end_inset

 give 
\begin_inset Formula $m_{1}>\frac{1}{2}n$
\end_inset

 and 
\begin_inset Formula $m_{1}>-n-1$
\end_inset

 which 
\series bold
can't be satisfied simultaneously
\series default
.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{1}-n>0$
\end_inset

,
 
\begin_inset Formula $-n>0$
\end_inset

 give 
\begin_inset Formula $m_{1}>\frac{1}{2}n$
\end_inset

.
 This will contribute for 
\begin_inset Formula $n<0$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{1}-n>0$
\end_inset

,
 
\begin_inset Formula $-m_{1}+n+1>0$
\end_inset

 give 
\begin_inset Formula $m_{1}>\frac{1}{2}n$
\end_inset

 which means 
\begin_inset Formula $n$
\end_inset

 must be negative and 
\begin_inset Formula $\frac{1}{2}n<m_{1}<n+1$
\end_inset

 which 
\series bold
can't be satisfied
\series default
.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $m_{1}+n+1>0$
\end_inset

 so 
\begin_inset Formula $m_{1}>-n-1$
\end_inset

 which gives points for 
\begin_inset Formula $n>0$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $m_{1}+n+1>0$
\end_inset

,
 
\begin_inset Formula $2m_{2}-n>0$
\end_inset

.
 Get 
\begin_inset Formula $m_{2}>\frac{1}{2}n$
\end_inset

 and 
\begin_inset Formula $m_{1}>-n-1$
\end_inset

 which is 
\series bold
impossible
\series default
.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $-n>0$
\end_inset

,
 
\begin_inset Formula $m_{1}+n+1>0$
\end_inset

.
 
\begin_inset Formula $m_{1}>-n-1>-1$
\end_inset

 so 
\series bold
doesn't contribute
\series default
.
\end_layout

\begin_layout Subsubsection*
Region 2
\end_layout

\begin_layout Standard
Take the singular case 
\begin_inset Formula $m_{1}=0,m_{2}<0$
\end_inset

.
 
\begin_inset Formula $\eta=\left(\epsilon,1\right)$
\end_inset

.
\begin_inset Formula 
\[
\text{Cone}\left(Q_{0,2},Q_{1,1}\right),\text{Cone}\left(Q_{0,2},Q_{2,0}\right),\text{Cone}\left(Q_{0,2},Q_{1,-1}\right),\text{Cone}\left(Q_{-1,1},Q_{1,1}\right),\text{Cone}\left(Q_{-1,1},Q_{2,0}\right),\text{Cone}\left(Q_{-2,0},Q_{1,1}\right).
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $2m_{2}-n>0$
\end_inset

,
 
\begin_inset Formula $ $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}y^{-1};q\right)}\right)^{-1}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{2}}y^{-1};q\right)}\right)^{-g_{2}-1}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}x_{2}^{g_{2}}y;q\right)}\right)^{2\left(-g_{2}+2\right)}
\]

\end_inset


\end_layout

\begin_layout Subsection*
truncation of sum (wrong but prob on the right path,
 I did this before learning JK-Res properly)
\end_layout

\begin_layout Standard
The naive condition for a pole is
\begin_inset Formula 
\[
g_{\alpha}^{j}m_{j}+a_{\alpha}^{l}n_{l}-r_{\alpha}\geq0.
\]

\end_inset

It is naive because in cases where more than one chiral multiplet is present,
 poles might come with a zero of another chiral which cancel each other and eventually there's no pole (eg 
\begin_inset Formula $\prod_{g=2,-2}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-g}y^{-1};q\right)}\right)^{-gm-n}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y;q\right)}\right)^{gm+n+1}$
\end_inset

).
 For each 
\begin_inset Formula $m^{j}$
\end_inset

 in the sum choose 
\begin_inset Formula $\eta^{j}=c^{\alpha}g_{\alpha}^{j}=-m^{j}$
\end_inset

.
 For that to work need to solve
\begin_inset Formula 
\[
\left(\begin{array}{ccc}
- & g^{1} & -\\
- & g^{2} & -\\
 & \vdots\\
- & g^{r_{G}} & -
\end{array}\right)\vec{c}=-\vec{m}\Rightarrow\vec{c}=-\left(\begin{array}{ccc}
- & g^{1} & -\\
- & g^{2} & -\\
 & \vdots\\
- & g^{r_{G}} & -
\end{array}\right)^{-1}\vec{m}\Rightarrow\vec{c}=\mathcal{O}\left(m\right).
\]

\end_inset

Need this for 
\begin_inset Formula $\alpha$
\end_inset

 which go over a set of 
\begin_inset Formula $r_{G}$
\end_inset

 charge covectors that their positive cone includes 
\begin_inset Formula $-\vec{m}$
\end_inset

.
 This also means 
\begin_inset Formula $c^{\alpha}>0$
\end_inset

 (it is not 
\begin_inset Formula $\geq$
\end_inset

 because we don't include the boundary of the cone,
 see footnote 8 in https://arxiv.org/pdf/1506.08795).
 Denote
\begin_inset Formula 
\[
Q=-\frac{1}{2}\left(\begin{array}{ccc}
- & g^{1} & -\\
- & g^{2} & -\\
 & \vdots\\
- & g^{r_{G}} & -
\end{array}\right)^{-1}\Rightarrow c^{\alpha}=2Q_{\ j}^{\alpha}m^{j}
\]

\end_inset

Note 
\begin_inset Formula $Q$
\end_inset

 takes a 
\begin_inset Formula $j$
\end_inset

 index related to the rank of the group,
 and returns an 
\begin_inset Formula $\alpha$
\end_inset

 index which is related to the charge covectors (which also has 
\begin_inset Formula $r_{G}$
\end_inset

 values,
 so it's a square matrix and an inverse makes sense).
 Then
\begin_inset Formula 
\[
g_{\alpha}^{j}m_{j}+a_{\alpha}^{l}n_{l}-r_{\alpha}\geq0
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow c^{\alpha}g_{\alpha}^{j}m_{j}+c^{\alpha}a_{\alpha}^{l}n_{l}-c^{\alpha}r_{\alpha}\geq0
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow-m_{j}^{2}+2Q_{\ j}^{\alpha}m^{j}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\geq0
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow\left(m_{j}-Q_{\ j}^{\alpha}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\right)^{2}-\left(Q_{\ j}^{\alpha}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\right)^{2}\leq0
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow\left(m_{j}-Q_{\ j}^{\alpha}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\right)^{2}\leq\left(Q_{\ j}^{\alpha}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\right)^{2}
\]

\end_inset

This should be done for each 
\begin_inset Formula $Q$
\end_inset

 in all cones.
 
\end_layout

\begin_layout Standard
Let's check this for our case above of SQCD.
 We have
\begin_inset Formula 
\[
\mathcal{I}\left(y,n\right)=\frac{1}{2}\sum_{m\in\mathbb{Z}}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2};q\right)}\right)^{2\left(gm-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y;q\right)}\right)^{4\left(gm+n+1\right)}
\]

\end_inset

The 
\begin_inset Formula $\vec{g}$
\end_inset

 are (We actually have a lot of degeneracy which should be more complicated,
 and are we only on the boundary of the cone which might be problematic?)
\begin_inset Formula 
\[
\vec{g}_{1}=\left(1\right),\vec{g}_{2}=\left(-1\right)
\]

\end_inset

For 
\begin_inset Formula $m>0$
\end_inset

,
 
\begin_inset Formula $\eta=-m<0\Rightarrow Q=-\frac{1}{2}g_{2}^{-1}=\frac{1}{2}$
\end_inset

.
 This 
\begin_inset Formula $g_{2}$
\end_inset

 corresponds to one of the chirals with their 
\begin_inset Formula $a$
\end_inset

 and 
\begin_inset Formula $r$
\end_inset

 which shouldn't matter,
 take eg the 
\begin_inset Formula $a=-2,r=1$
\end_inset

 case and later check the 
\begin_inset Formula $a=1,r=0$
\end_inset

 case.
\begin_inset Formula 
\[
\left(m-\frac{1}{2}\left(-2n_{l}-1\right)\right)^{2}\leq\left(\frac{1}{2}\left(-2n_{l}-1\right)\right)^{2}\Rightarrow\left(m+n+\frac{1}{2}\right)^{2}\leq\left(n+\frac{1}{2}\right)^{2}
\]

\end_inset

For 
\begin_inset Formula $n>0$
\end_inset

 note 
\begin_inset Formula $\left(m+n+\frac{1}{2}\right)^{2}\geq m^{2}+\left(n+\frac{1}{2}\right)^{2}\geq\left(n+\frac{1}{2}\right)^{2}$
\end_inset

 thus no contribution.
 Graphically,
 we have a sphere with 
\begin_inset Formula $r=n+\frac{1}{2}$
\end_inset

 with its origin at 
\begin_inset Formula $-\left(n+\frac{1}{2}\right)$
\end_inset

,
 thus 
\begin_inset Formula $m>0$
\end_inset

 can't be inside.
 For 
\begin_inset Formula $n<0$
\end_inset

 can write
\begin_inset Formula 
\[
\left(n+\frac{1}{2}\right)^{2}=\left(\left|n\right|-\frac{1}{2}\right)^{2},\left(m+n+\frac{1}{2}\right)^{2}=\left(m-\left(\left|n\right|-\frac{1}{2}\right)\right)^{2}
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow\left(m-\left(\left|n\right|-\frac{1}{2}\right)\right)^{2}\leq\left(\left|n\right|-\frac{1}{2}\right)^{2}
\]

\end_inset

which always gets contributions for 
\begin_inset Formula $0<m<2\left(\left|n\right|-\frac{1}{2}\right)=-2n-1$
\end_inset

.
 Let's check what happens if we chose 
\begin_inset Formula $a=1$
\end_inset

,
\begin_inset Formula $r=0$
\end_inset

 instead
\begin_inset Formula 
\[
\left(m-\frac{1}{2}n\right)^{2}\leq\left(\frac{1}{2}n\right)^{2}.
\]

\end_inset

Here 
\begin_inset Formula $n>0$
\end_inset

 contributes for 
\begin_inset Formula $0<m<n$
\end_inset

 and 
\begin_inset Formula $n<0$
\end_inset

 doesn't contribute,
 because of the origin and the radius 
\begin_inset Formula $m$
\end_inset

 needs to be in.
 
\begin_inset Newline newline
\end_inset

For 
\begin_inset Formula $m<0$
\end_inset

,
 
\begin_inset Formula $\eta=-m>0\Rightarrow Q=-\frac{1}{2}g_{1}^{-1}=-\frac{1}{2}$
\end_inset

.
\begin_inset Formula 
\[
\left(m_{j}-Q_{\ j}^{\alpha}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\right)^{2}\leq\left(Q_{\ j}^{\alpha}\left(a_{\alpha}^{l}n_{l}-r_{\alpha}\right)\right)^{2}
\]

\end_inset

 
\end_layout

\begin_layout Subsection*
special case 
\begin_inset Formula $m_{1}=m_{2}=1,n=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\#\prod_{g_{1},g_{2}=2,0,-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}y^{-1};q\right)}\right)^{g_{1}}\left(\frac{1}{\theta_{1}\left(x_{2}^{g_{2}}y^{-1};q\right)}\right)^{g_{2}}\prod_{g_{1},g_{2}=\pm1}\left(\frac{1}{\theta_{1}\left(x_{1}^{g_{1}}x_{2}^{g_{2}}y;q\right)}\right)^{2\left(g_{1}+g_{2}+1\right)}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
 & \frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{2}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}^{-1}y;q\right)}\right)^{-2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{2}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}y;q\right)}\right)^{6}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}^{-1}y;q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}y;q\right)}\right)^{2}
\end{align*}

\end_inset

The charge covectors are 
\begin_inset Formula $Q_{3}=\left(2,0\right),Q_{4}=\left(0,2\right),Q_{5}=\left(1,1\right),Q_{2}=\left(1,-1\right),Q_{1}=\left(-1,1\right)$
\end_inset

.
 The hyperplanes are
\begin_inset Formula 
\[
H_{1}:-x+y+v=0
\]

\end_inset


\begin_inset Formula 
\[
H_{2}:x-y+v=0
\]

\end_inset


\begin_inset Formula 
\[
H_{3}:2x-v=0
\]

\end_inset


\begin_inset Formula 
\[
H_{4}:2y-v=0
\]

\end_inset


\begin_inset Formula 
\[
H_{5}:x+y+v=0
\]

\end_inset

where the identifications on 
\begin_inset Formula $\mathfrak{M}$
\end_inset

 are realized.
 Choose 
\begin_inset Formula $\eta=\left(-\frac{1}{2},\frac{3}{2}\right)$
\end_inset

.
 which is in the chambers relevant to 
\begin_inset Formula $\text{Cone}\left(Q_{1},Q_{4}\right),\text{Cone}\left(Q_{1},Q_{3}\right),\text{Cone}\left(Q_{1},Q_{5}\right)$
\end_inset

.
 The intersection between 
\begin_inset Formula $H_{1},H_{4}$
\end_inset

 give 4 points
\begin_inset Formula 
\[
x=\frac{a}{2}+\frac{b}{2}\tau+\frac{3}{2}v,y=\frac{1}{2}\left(v+a+b\tau\right),a,b=0,1.
\]

\end_inset

The residue is
\begin_inset Formula 
\begin{align*}
 & \frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{2}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}^{-1}y;q\right)}\right)^{-2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{2}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}y;q\right)}\right)^{6}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}^{-1}y;q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}y;q\right)}\right)^{2}
\end{align*}

\end_inset

the intersection between 
\begin_inset Formula $H_{1},H_{3}$
\end_inset

 give 4 points
\begin_inset Formula 
\[
y=\frac{1}{2}\left(a+b\tau\right),x=v+\frac{1}{2}\left(a+b\tau\right),a,b=0,1.
\]

\end_inset

The residue is
\begin_inset Formula 
\begin{align*}
 & \frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{2}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}^{-1}y;q\right)}\right)^{-2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{2}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}y;q\right)}\right)^{6}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}^{-1}y;q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}y;q\right)}\right)^{2}
\end{align*}

\end_inset

the intersection between 
\begin_inset Formula $H_{1},H_{5}$
\end_inset

 give a single point
\begin_inset Formula 
\[
x=0,y=-v.
\]

\end_inset

The residue is
\begin_inset Formula 
\begin{align*}
 & \frac{1}{2}\frac{1}{2\pi i}\int i\eta\left(q\right)^{4}\d u_{1}du_{2}\prod_{i=1}^{2}\frac{\theta_{1}\left(x_{i}^{2};q\right)\theta_{1}\left(x_{i}^{-2};q\right)}{\left(i\eta\left(q\right)\right)^{2}}\left(f_{\chi}\left(0,-2,2\right)\right)^{2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{2}^{-2}y^{-1};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}^{-1}y;q\right)}\right)^{-2}\\
 & \left(\frac{1}{\theta_{1}\left(x_{1}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{2}^{2}y^{-1};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}y;q\right)}\right)^{6}\left(\frac{1}{\theta_{1}\left(x_{1}x_{2}^{-1}y;q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x_{1}^{-1}x_{2}y;q\right)}\right)^{2}
\end{align*}

\end_inset


\end_layout

