\begin_layout Standard
For 
\begin_inset Formula $\zeta=1$
\end_inset


\begin_inset Formula 
\[
Z_{\text{chiral}}=\prod_{\rho\in R}\left(\frac{i\eta}{\theta_{1}\left(x^{\rho};q\right)}\right)^{\rho\left(m\right)-r+1}
\]

\end_inset

Field contents are 
\begin_inset Formula $SU\left(2\right)$
\end_inset

 gauge field and 6 chirals in the fundamental.
 The twisted R-charge is 
\begin_inset Formula 
\begin{align*}
 & \frac{1}{2}\sum_{m\in\mathbb{Z}}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2};q\right)}\right)^{2\left(gm-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y;q\right)}\right)^{4\left(gm+n+1\right)}\\
 & =\frac{1}{2}\sum_{m\in\mathbb{Z}}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\\
 & \times\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy^{-2};q\right)}\right)^{2\left(m-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{2\left(-m-2n\right)}\\
 & \times\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{4\left(m+n+1\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{4\left(-m+n+1\right)}\\
 & =\frac{1}{2\pi i}\int\d u\sum_{m\geq-M}I(q,y;n)\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy^{-2};q\right)}\right)^{2m}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{-2m}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{4m}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{-4m}\\
 & =\frac{1}{2\pi i}\int\d u\sum_{m\geq0}I(x,y;n)\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy^{-2};q\right)}\right)^{2m}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{-2m}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{4m}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{-4m}\\
 & \times\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy^{-2};q\right)}\right)^{-2M}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{2M}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{-4M}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{4M}\\
 & =\frac{1}{2\pi i}\int\d uI(x,y;n)\frac{G\left(x,y;M\right)}{1-F\left(x,y;q\right)}
\end{align*}

\end_inset

For,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
F\left(x,y;q\right) & =\left(\frac{1}{\theta_{1}\left(xy^{-2};q\right)}\right)^{2}\left(\frac{1}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{-2}\left(\frac{1}{\theta_{1}\left(xy;q\right)}\right)^{4}\left(\frac{1}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{-4}\\
 & =\frac{\theta_{1}\left(x^{-1}y^{-2};q\right)^{2}\theta_{1}\left(x^{-1}y;q\right)^{4}}{\theta_{1}\left(xy^{-2};q\right)^{2}\theta_{1}\left(xy;q\right)^{4}}
\end{align*}

\end_inset


\begin_inset Formula 
\[
G\left(x,y;M\right)=\left(i\eta\left(q\right)F\left(x,y\right)\right)^{-M}=\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy^{-2};q\right)}\right)^{-2M}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{2M}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{-4M}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{4M}
\]

\end_inset


\begin_inset Formula 
\[
I\left(x,y;n\right)=\frac{1}{2}i\eta\left(q\right)^{2}\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(q\right)}\times\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy^{-2};q\right)}\right)^{2\left(-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y^{-2};q\right)}\right)^{2\left(-2n\right)}\times\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{4\left(n+1\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{4\left(n+1\right)}
\]

\end_inset

Expand for small 
\begin_inset Formula $q$
\end_inset

,
 at 0th order
\begin_inset Formula 
\[
F\approx\left(\frac{(x-y)^{2}\left(xy^{2}-1\right)}{(xy-1)^{2}\left(x-y^{2}\right)}\right)^{2}
\]

\end_inset


\begin_inset Formula 
\[
\frac{1}{2\pi i}\int\d uI(x,y;n)\frac{G\left(x,y;M\right)}{1-F\left(x,y;q\right)}\approx\frac{1}{2\pi i}\int\d uI(x,y;n)\frac{\left(\left(\frac{(x-y)^{2}\left(xy^{2}-1\right)}{(xy-1)^{2}\left(x-y^{2}\right)}\right)^{2}\right)^{-M}}{1-\left(\frac{(x-y)^{2}\left(xy^{2}-1\right)}{(xy-1)^{2}\left(x-y^{2}\right)}\right)^{2}}
\]

\end_inset


\end_layout

\begin_layout Section*
Truncation of sum
\end_layout

\begin_layout Standard
For rank 1 gauge theories
\begin_inset Formula 
\[
\mathcal{I}\left(\mu_{a},s_{a}\right)=\sum_{m\in\mathbb{Z}}\oint_{C}\frac{dz}{z}\prod_{\alpha}\left(\frac{1}{\theta\left(z^{e_{\alpha}}\mu_{a}^{f_{\alpha,a}};q\right)}\right)^{e_{\alpha}m+f_{\alpha,a}s_{a}+1-r_{\alpha}}
\]

\end_inset

where 
\begin_inset Formula $e_{\alpha},f_{\alpha,a}$
\end_inset

 are the gauge and flavor charges of the 
\begin_inset Formula $\alpha$
\end_inset

th chiral,
 and 
\begin_inset Formula $r_{\alpha}$
\end_inset

 its R-charge.
 
\begin_inset Formula $m$
\end_inset

 is the flux for the single cartan and 
\begin_inset Formula $s_{a}$
\end_inset

 is for the flavor cartans (
\begin_inset Formula $a$
\end_inset

 ranges the rank of the flavor group).
 C encircles poles contributed by chirals with 
\begin_inset Formula $e_{\alpha}>0$
\end_inset

 (with some orientation),
 or those contributed by chirals with 
\begin_inset Formula $e_{\alpha}<0$
\end_inset

 (with opposite orientation).
 In our case we have 6 chirals,
 each having two weights as they are in the fundamental of 
\begin_inset Formula $SU\left(2\right)$
\end_inset

 (the two weights being 
\begin_inset Formula $\pm1$
\end_inset

),
 thus 
\begin_inset Formula $\alpha$
\end_inset

 goes from 1 to 12.
 The R-charge (which is originally 
\begin_inset Formula $\frac{1}{3}$
\end_inset

) is mixed with one cartan of the flavor 
\begin_inset Formula $\left(\frac{2}{3},\frac{2}{3},-\frac{1}{3},-\frac{1}{3},-\frac{1}{3},-\frac{1}{3}\right)$
\end_inset

 such that it is broken to 
\begin_inset Formula $\left(1,1,0,0,0,0\right)$
\end_inset

.
 Flavor flux is only turned on for that same cartan,
 which for some reason is now mentioned as 
\begin_inset Formula $\left(-2,-2,1,1,1,1\right)$
\end_inset

 (different by a factor of 
\begin_inset Formula $-\frac{1}{3}$
\end_inset

).
 The mixing breaks the flavor symmetry to 
\begin_inset Formula $SU\left(2\right)\times SU\left(4\right)\times U\left(1\right)\subset SU\left(6\right)$
\end_inset

.
 Because of the single flux,
 we only have 
\begin_inset Formula $f_{\alpha,a}s_{a}=f_{\alpha}s$
\end_inset

 (only a single value for 
\begin_inset Formula $a$
\end_inset

).
 For expressions to stay relatively compact we use the definition
\begin_inset Formula 
\[
h\left(e,f,r\right)=\left(\frac{1}{\theta\left(z^{e}\mu^{f};q\right)}\right)^{em+fs+1-r},
\]

\end_inset

and the partition function is
\begin_inset Formula 
\[
\mathcal{I}\left(\mu_{a},s_{a}\right)=\sum_{m\in\mathbb{Z}}\oint_{C}\frac{dz}{z}\prod_{e=\pm1}h\left(e,-2,1\right)^{2}h\left(e,1,0\right)^{4}.
\]

\end_inset

Poles contribute only for
\begin_inset Formula 
\[
e_{\alpha}m+f_{\alpha}s-r_{\alpha}\geq0,\left|e_{\alpha}\right|=1\Rightarrow\begin{cases}
m\geq\left(-f_{\alpha}s+r_{\alpha}\right) & e_{\alpha}>0\\
m\leq\left(f_{\alpha}s-r_{\alpha}\right) & e_{\alpha}<0
\end{cases},
\]

\end_inset

or after plugging in our case of flavor/R charges
\begin_inset Formula 
\[
\begin{cases}
m\geq\left(2s+1\right) & e_{\alpha}>0\\
m\leq\left(-2s-1\right) & e_{\alpha}<0
\end{cases}\text{ for }\left(f,r\right)=\left(-2,1\right)\ ,\ \begin{cases}
m\geq-s & e_{\alpha}>0\\
m\leq s & e_{\alpha}<0
\end{cases}\text{ for }\left(f,r\right)=\left(1,0\right).
\]

\end_inset

Let's look at different cases.
 For 
\begin_inset Formula $s=0$
\end_inset

,
 for 
\begin_inset Formula $m<0$
\end_inset

 we can choose 
\begin_inset Formula $e>0$
\end_inset

 and there will be no poles,
 and for 
\begin_inset Formula $m>0$
\end_inset

 we can choose 
\begin_inset Formula $e<0$
\end_inset

 and again no pole will contribute.
 Thus we only have 
\begin_inset Formula $m=0$
\end_inset


\begin_inset Formula 
\begin{align*}
\mathcal{I}\left(\mu,s\right) & =\oint_{C}\frac{dz}{z}\prod_{e=\pm1}h\left(e,-2,1\right)^{2}h\left(e,1,0\right)^{4}\\
 & =\oint_{C}\frac{dz}{z}\prod_{e=\pm1}\left(\frac{1}{\theta\left(z^{e}\mu^{-2};q\right)}\right)^{0\cdot2}\left(\frac{1}{\theta\left(z^{e}\mu;q\right)}\right)^{1\cdot4}\\
 & =\oint_{C}\frac{dz}{z}\left(\frac{1}{\theta\left(z\mu;q\right)}\right)^{4}\left(\frac{1}{\theta\left(z^{-1}\mu;q\right)}\right)^{4}
\end{align*}

\end_inset

and we see how only 
\begin_inset Formula $r=0$
\end_inset

 contribute poles and 
\begin_inset Formula $r=1$
\end_inset

 don't contribute at all.
 
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
The upper bound of our sum is given by
\begin_inset Formula 
\[
m\leq\max\left\{ 2s-1,s\right\} =\begin{cases}
s & s\leq0,\text{only }\left(f,r\right)=\left(1,0\right)\\
1 & s=1,\text{both types contribute}\\
2s-1 & s\geq2,\text{only }\left(f,r\right)=\left(-2,1\right)
\end{cases}
\]

\end_inset

Similarly,
 the lower bound is
\end_layout

\end_inset

This is actually not quite right,
 start with
\begin_inset Formula 
\[
\mathcal{I}\left(y,n\right)=\frac{1}{2}\sum_{m\in\mathbb{Z}}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2};q\right)}\right)^{2\left(gm-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y;q\right)}\right)^{4\left(gm+n+1\right)}
\]

\end_inset

Now again plug in 
\begin_inset Formula $n=0$
\end_inset

,
 and only 
\begin_inset Formula $m=0$
\end_inset

 should contribute
\begin_inset Formula 
\[
\mathcal{I}\left(y,0\right)=\frac{1}{2}\cdot\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(xy;q\right)}\right)^{4}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{-1}y;q\right)}\right)^{4}
\]

\end_inset

Expanding in q and taking the residue at 
\begin_inset Formula $x=y$
\end_inset

 we get the expected expression.
 
\end_layout

\begin_layout Standard
Moving on to 
\begin_inset Formula $n\neq0$
\end_inset

.
 For 
\begin_inset Formula $n>0$
\end_inset

 still 
\begin_inset Formula $\left(f,r\right)=\left(-2,1\right)$
\end_inset

 never contributes,
 and now 
\begin_inset Formula $-n\leq m\leq n$
\end_inset

 has poles from 
\begin_inset Formula $\left(f,r\right)=\left(1,0\right)$
\end_inset

.
 This actually generalizes the 
\begin_inset Formula $n=0$
\end_inset

 case and we can include it as part of the equation
\begin_inset Formula 
\[
\mathcal{I}\left(y,n\geq0\right)=\frac{1}{2}\sum_{m=-n}^{n}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2};q\right)}\right)^{2\left(gm-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y;q\right)}\right)^{4\left(gm+n+1\right)}.
\]

\end_inset

On the other hand,
 for 
\begin_inset Formula $n<0$
\end_inset

,
 poles are now contributed by 
\begin_inset Formula $\left(f,r\right)=\left(-2,1\right)$
\end_inset

 for 
\begin_inset Formula $2n+1\leq m\leq-2n-1$
\end_inset

,
 while 
\begin_inset Formula $\left(f,r\right)=\left(1,0\right)$
\end_inset

 doesn't contribute poles
\begin_inset Formula 
\[
\mathcal{I}\left(y,n\right)=\frac{1}{2}\sum_{m=-2n-1}^{2n+1}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2};q\right)}\right)^{2\left(gm-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y;q\right)}\right)^{4\left(gm+n+1\right)}.
\]

\end_inset


\end_layout

\begin_layout Section*
Fugacity
\end_layout

\begin_layout Standard
The global symmetry is 
\begin_inset Formula $SU\left(2\right)\times SU\left(4\right)\times U\left(1\right)\subset SU\left(6\right)$
\end_inset

.
 We know 
\begin_inset Formula $\boldsymbol{6}\to\left(\boldsymbol{4},\boldsymbol{1}\right)_{1}\oplus\left(\boldsymbol{1},\boldsymbol{2}\right)_{-2}$
\end_inset

,
 let's check
\begin_inset Formula 
\begin{align*}
\chi_{\boldsymbol{6}}\left(\boldsymbol{a}\right) & =a_{1}+...+a_{6}\\
 & =\left(a_{1}+a_{2}\right)+\left(a_{3}+a_{4}+a_{5}+a_{6}\right)\\
 & =a^{x}\left(c_{1}+c_{2}\right)+a^{y}\left(d_{1}+d_{2}+d_{3}+d_{4}\right)
\end{align*}

\end_inset

where e.g.
\begin_inset Formula 
\[
c_{2}=\frac{1}{c_{1}}\Rightarrow a_{2}a^{-x}=\frac{1}{a_{1}a^{-x}}\Rightarrow a^{x}=\left(a_{1}a_{2}\right)^{1/2}
\]

\end_inset

and
\begin_inset Formula 
\[
a^{2x+4y}=1\Rightarrow\frac{x}{y}=-2
\]

\end_inset

meaning 
\begin_inset Formula $x=-2,y=1$
\end_inset

 is the right ratio
\begin_inset Formula 
\[
\chi_{\boldsymbol{6}}\left(\boldsymbol{a}\right)=a^{-2}\left(c_{1}+c_{2}\right)+a^{1}\left(d_{1}+d_{2}+d_{3}+d_{4}\right).
\]

\end_inset

We still have a cartan of 
\begin_inset Formula $SU\left(2\right)$
\end_inset

 and 3 cartans of 
\begin_inset Formula $SU\left(4\right)$
\end_inset

.
 
\begin_inset Formula 
\[
\left(\boldsymbol{1},\boldsymbol{2}\right)_{-2}\to\left(1,-2\right)\oplus\left(-1,-2\right)
\]

\end_inset


\begin_inset Formula 
\[
\left(\boldsymbol{4},\boldsymbol{1}\right)_{1}\to\left(1,0,0,1\right)\oplus\left(0,1,0,1\right)\oplus\left(0,0,1,1\right)\oplus\left(-1,-1,-1,1\right)
\]

\end_inset

In characters language
\begin_inset Formula 
\begin{align*}
\chi_{\boldsymbol{6}}\left(\boldsymbol{a}\right) & =a^{-2}\left(c_{1}+c_{2}\right)+a^{1}\left(d_{1}+d_{2}+d_{3}+d_{4}\right)\\
 & =a^{-2}c+a^{-2}c^{-1}+a^{1}\left(f_{1}^{x}+f_{2}^{y}+f_{3}^{z}+f_{1}^{-x}f_{2}^{-y}f_{3}^{-z}\right)
\end{align*}

\end_inset

choose 
\begin_inset Formula $x=y=z=1$
\end_inset


\begin_inset Formula 
\[
\chi_{\boldsymbol{6}}\left(\boldsymbol{a}\right)=a^{-2}c+a^{-2}c^{-1}+a^{1}f_{1}+a^{1}f_{2}+a^{1}f_{3}+a^{1}f_{1}^{-1}f_{2}^{-1}f_{3}^{-1}.
\]

\end_inset


\end_layout

\begin_layout Subsection*
Not dual
\end_layout

\begin_layout Standard
Turning on the fugacities for the 4 cartans,
 the index turns into
\begin_inset Formula 
\begin{align*}
\mathcal{I}\left(y,n\right) & =\frac{1}{2}\sum_{m=-2n-1}^{2n+1}\frac{1}{2\pi i}\int i\eta\left(a\right)^{2}\d u\frac{\theta_{1}\left(x^{2};q\right)\theta_{1}\left(x^{-2};q\right)}{i\eta\left(a\right)}\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2}c;q\right)}\right)^{\left(gm-2n\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}y^{-2}c^{-1};q\right)}\right)^{\left(gm-2n\right)}\\
 & \times\prod_{g=\pm1}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}yf_{1};q\right)}\right)^{\left(gm+n+1\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}yf_{2};q\right)}\right)^{\left(gm+n+1\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}yf_{3};q\right)}\right)^{\left(gm+n+1\right)}\left(\frac{i\eta\left(q\right)}{\theta_{1}\left(x^{g}yf_{1}^{-1}f_{2}^{-1}f_{3}^{-1};q\right)}\right)^{\left(gm+n+1\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection*
Dual
\end_layout

\begin_layout Standard
Before activating fugacities:
\begin_inset Formula 
\[
6\to\left(4,1\right)_{+1}+\left(1,2\right)_{-2}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
6\otimes_{A}6 & \to\left(\left(4,1\right)_{+1}+\left(1,2\right)_{-2}\right)\otimes_{A}\left(\left(4,1\right)_{+1}+\left(1,2\right)_{-2}\right)\\
 & =\left(6,1\right)_{+2}+\left(4,2\right)_{-1}+\left(1,1\right)_{-4}
\end{align*}

\end_inset

* Why only one 
\begin_inset Formula $\left(4,2\right)_{-1}$
\end_inset

?
\begin_inset Newline newline
\end_inset

After fugacities:
\begin_inset Formula 
\[
\chi_{\left(6,1\right)_{+2}}\left(a,\boldsymbol{f},c\right)=a^{+2}\left(f_{1}f_{2}+f_{1}f_{3}+f_{1}f_{4}+f_{2}f_{3}+f_{2}f_{4}+f_{3}f_{4}\right)
\]

\end_inset


\begin_inset Formula 
\[
\chi_{\left(1,1\right)_{-4}}\left(a,\boldsymbol{f},c\right)=a^{-4}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\chi_{\left(4,2\right)_{-1}}\left(a,\boldsymbol{f},c\right) & =a^{-1}\left(f_{1}+f_{2}+f_{3}+f_{4}\right)\left(c+c^{-1}\right)\\
 & =a^{-1}f_{1}\left(c+c^{-1}\right)+a^{-1}f_{2}\left(c+c^{-1}\right)+a^{-1}f_{3}\left(c+c^{-1}\right)+a^{-1}f_{4}\left(c+c^{-1}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Section*
